    Private Function PositionConnexionDansListeConnexion(ByVal selected_string As String)
        Dim Position As Integer = 0
        Dim String_Temporaire = Nothing

        While (String_Temporaire <> selected_string)
            String_Temporaire = Form_Demarrage.Liste_Connexions(Position, 1)

            If (String_Temporaire <> selected_string) Then
                Position = Position + 1
            End If

        End While

        Return Position
    End Function


