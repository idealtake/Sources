            Me.Etat_Ajout_Suppression_TextBox.Text = Me.Etat_Ajout_Suppression_TextBox.Text & MessageUtilisateur(192, Form_Demarrage.LangueLogiciel) & "`TABLE_E_vue_recherche_italien` : " + ex.Message
            Return True
        Finally
            If Not reader_22_02_2011y Is Nothing Then reader_22_02_2011y.Close()
        End Try


        conn.Close() '' on ferme la connection

        Return True
    End Function

    Private Sub GetDatabases()

        Dim reader As MySqlDataReader

        Dim i As Integer = 1
        Dim base_name As String = ""
        reader = Nothing

        Dim cmd As New MySqlCommand("SHOW DATABASES", conn)
        Try
            reader = cmd.ExecuteReader()
            Me.Etat_Ajout_Suppression_TextBox.Clear()
            Me.Etat_Ajout_Suppression_TextBox.TextAlign = HorizontalAlignment.Left
            Me.Etat_Ajout_Suppression_TextBox.Text = "Base(s) présente(nt) : " & vbCrLf & vbCrLf

            While (reader.Read())
                base_name = reader.GetString(0)
                Me.Etat_Ajout_Suppression_TextBox.Text = Me.Etat_Ajout_Suppression_TextBox.Text & " -> " & base_name & vbCrLf
                'on enregistre dans un tableau la liste des bases rrtrouvé sur ce serveur
                data_base_list(i, 1) = base_name 'on stocke l'information dans une table
                i += 1
            End While

        Catch ex As MySqlException
            MessageBox.Show("Impossible de déterminer la liste des bases de données : " + ex.Message)
        Finally
            If Not reader Is Nothing Then reader.Close()
        End Try


    End Sub


