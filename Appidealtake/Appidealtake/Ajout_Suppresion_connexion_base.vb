    Function ProcédureEntreServeurEtDonnéesLocale(ByVal Mysql_Connection As MySql.Data.MySqlClient.MySqlConnection, _
                                                       ByVal Chaine_de_connexion_Mysql As String, ByVal nom_de_la_base As String, _
                                                       ByVal Adresse_Serveur As String, ByVal Langue_Logiciel As Integer)

        Dim VERSIONS_TABLE_Base_de_données As New CLASS_IDEALTAKE.CLASS_IDEALTAKE.Objects.VERSIONS_TABLES
        Dim VERSIONS_TABLE_Données_locales As New CLASS_IDEALTAKE.CLASS_IDEALTAKE.Objects.VERSIONS_TABLES
        Dim chemin_complet_nom_répertoire_xml As String
        Dim chemin_du_répertoire_des_données_locales_xml As String
        Dim Nom_du_répertoire_xml As String

        Dim log_connexion As String = ""

        'PHASE 1 
        'Ouverture de la connexion avec le serveur
        Try
            'log
            log_connexion = Chaine_de_connexion_Mysql

            Mysql_Connection.ConnectionString = Chaine_de_connexion_Mysql
            Mysql_Connection.Open()
            '"Connexion Ouverte..."


            Select Case Langue_Logiciel
                Case 0
                    log_connexion = log_connexion & vbCrLf & "Geöffnete Verbindung..." & vbCrLf
                Case 1
                    log_connexion = log_connexion & "Opened connection..." & vbCrLf
                Case 2
                    log_connexion = log_connexion & vbCrLf & "Conexión Abierta..." & vbCrLf
                Case 3
                    log_connexion = log_connexion & vbCrLf & "Connexion Ouverte..." & vbCrLf
                Case 4
                    log_connexion = log_connexion & vbCrLf & "Connessione Aperta..." & vbCrLf
            End Select

        Catch ex As System.Exception


            Select Case Langue_Logiciel
                Case 0
                    log_connexion = log_connexion & vbCrLf & "Ein Irrtum hat sich ereignet : " & ex.Message & vbCrLf & _
                                    "Während der Verbindungsöffnung mit dem Server."
                Case 1
                    log_connexion = log_connexion & "An error occurred : " & ex.Message & vbCrLf & _
                                    "During connection with the server."
                Case 2
                    log_connexion = log_connexion & vbCrLf & "Un error se produjo : " & ex.Message & vbCrLf & _
                                    "Durante la apertura de conexión MySql."
                Case 3
                    log_connexion = log_connexion & vbCrLf & "Une erreur s'est produite : " & ex.Message & vbCrLf & _
                                    "Durant l'ouverture de connexion avec le serveur."
                Case 4
                    log_connexion = log_connexion & vbCrLf & "Un errore si è prodursi : " & ex.Message & vbCrLf & _
                                    "Durante l'apertura di connessione col server"
            End Select

            ' il faut sortir de la procédure le serveur n'est pas disponible
            Attente_Chargement.Affichage_Etat_chargement(4)
            Dim pauseTime As Integer = 800
            System.Threading.Thread.Sleep(pauseTime)
            chemin_complet_nom_répertoire_xml = My.Application.Info.DirectoryPath & "\local_data\" & Form_Demarrage.Calcul_du_nom_du_fichier_de_données_local(nom_de_la_base, Adresse_Serveur)
            Form_Demarrage.Log_MYSQL_VERS_XML(chemin_complet_nom_répertoire_xml, log_connexion & vbCrLf)
            Return False

        End Try


        'PHASE 2
        'Analyse du Répertoire


        Nom_du_répertoire_xml = Form_Demarrage.Calcul_du_nom_du_fichier_de_données_local(nom_de_la_base, Adresse_Serveur)
        'My.Application.Info.DirectoryPath & 
        chemin_du_répertoire_des_données_locales_xml = "local_data"
        chemin_complet_nom_répertoire_xml = My.Application.Info.DirectoryPath & "\local_data\" & Nom_du_répertoire_xml
        Dim résultat_Analyse_répertoire As String = ""

        'Analyse du nom du répertoire
        résultat_Analyse_répertoire = Form_Demarrage.Analyse_répertoire(chemin_du_répertoire_des_données_locales_xml, Nom_du_répertoire_xml, Langue_Logiciel)
        Form_Demarrage.Log_MYSQL_VERS_XML(chemin_complet_nom_répertoire_xml, log_connexion & vbCrLf & résultat_Analyse_répertoire)


        'PHASE 3
        'Analyse M.A.J (mise à jour)


        Dim résultat_Analyse_MAJ As String = ""

        résultat_Analyse_MAJ = Form_Demarrage.Analyse_MAJ_xml(chemin_du_répertoire_des_données_locales_xml, Nom_du_répertoire_xml, Mysql_Connection, _
                                                          VERSIONS_TABLE_Données_locales, VERSIONS_TABLE_Base_de_données, Langue_Logiciel)

        Form_Demarrage.Log_MYSQL_VERS_XML(chemin_complet_nom_répertoire_xml, résultat_Analyse_MAJ)



        'PHASE 4
        'Lancement M.A.J

        Dim Résultat_Lancement_MAJ As String = ""

        Résultat_Lancement_MAJ = Form_Demarrage.MajXmlLocaux(chemin_du_répertoire_des_données_locales_xml, Nom_du_répertoire_xml, Mysql_Connection,
                       VERSIONS_TABLE_Données_locales, VERSIONS_TABLE_Base_de_données, Langue_Logiciel)

        Form_Demarrage.Log_MYSQL_VERS_XML(chemin_complet_nom_répertoire_xml, Résultat_Lancement_MAJ)


        'PHASE 5
        'Chargement des fichiers xml dans les structures de Données : Datatable et Table

        Dim Résultat_Chargement_Données As String = ""
        Résultat_Chargement_Données = Form_Demarrage.Chargement_des_données_locales(chemin_complet_nom_répertoire_xml, Langue_Logiciel)

        Form_Demarrage.Log_MYSQL_VERS_XML(chemin_complet_nom_répertoire_xml, Résultat_Chargement_Données)


        'PHASE 6
        'Fermeture de la connexion avec le serveur

        Dim log_Fermeture_connexion As String = ""

        Try

            Mysql_Connection.Close()
            'Connexion Fermée...

            Select Case Langue_Logiciel
                Case 0
                    log_Fermeture_connexion = log_Fermeture_connexion & vbCrLf & "Geschlossene Verbindung..." & vbCrLf

                Case 1
                    log_Fermeture_connexion = log_Fermeture_connexion & vbCrLf & "Closed connection..." & vbCrLf

                Case 2
                    log_Fermeture_connexion = log_Fermeture_connexion & vbCrLf & "Conexión Cerrada..." & vbCrLf

                Case 3
                    log_Fermeture_connexion = log_Fermeture_connexion & vbCrLf & "Connexion Fermée..." & vbCrLf

                Case 4
                    log_Fermeture_connexion = log_Fermeture_connexion & vbCrLf & "Connessione Chiusa..." & vbCrLf

            End Select

            Form_Demarrage.Log_MYSQL_VERS_XML(chemin_complet_nom_répertoire_xml, log_Fermeture_connexion)

            Attente_Chargement.Affichage_Etat_chargement(2) 'Chargement terminé.
            Dim pauseTime As Integer = 400
            System.Threading.Thread.Sleep(pauseTime)

        Catch ex As Exception

            Select Case Langue_Logiciel
                Case 0
                    log_Fermeture_connexion = log_Fermeture_connexion & vbCrLf & "Ein Irrtum hat sich ereignet : " & ex.Message & vbCrLf & _
                                    "Während des Verbindungsverschlusses."
                Case 1
                    log_Fermeture_connexion = log_Fermeture_connexion & vbCrLf & "An error occurred : " & ex.Message & vbCrLf & _
                                    "During the closure of connection with the server."
                Case 2
                    log_Fermeture_connexion = log_Fermeture_connexion & vbCrLf & "Un error se produjo : " & ex.Message & vbCrLf & _
                                    "Durante el cierre de conexión MySql."
                Case 3
                    log_Fermeture_connexion = log_Fermeture_connexion & vbCrLf & "Une erreur s'est produite : " & ex.Message & vbCrLf & _
                                    "Durant la fermeture de connexion avec le serveur."
                Case 4
                    log_Fermeture_connexion = log_Fermeture_connexion & vbCrLf & "Un errore si è prodursi : " & ex.Message & vbCrLf & _
                                    "Durante la chiusura di connessione col server."
            End Select

            Form_Demarrage.Log_MYSQL_VERS_XML(chemin_complet_nom_répertoire_xml, log_Fermeture_connexion)
        End Try

        Return True
    End Function




