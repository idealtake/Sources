    Public Function CréationBase(ByVal Nom_de_l_hote As String, ByVal Nom_Utilisateur As String, ByVal mot_de_passe As String, ByVal Nom_de_la_base As String)




        If Not conn Is Nothing Then conn.Close()

        Dim connStr As String
        connStr = String.Format("server={0};user id={1}; password={2}; database=mysql; pooling=false", _
    Nom_de_l_hote, Nom_Utilisateur, mot_de_passe)

        Try
            conn = New MySqlConnection(connStr)
            conn.Open()

            GetDatabases()
        Catch ex As MySqlException
            MessageBox.Show(MessageUtilisateur(0, Form_Demarrage.LangueLogiciel) + ex.Message)
            Return True
        End Try


        ''on va vérifier que la base que l'on désire créer
        ''n'est pas déjà existante, si c'est le cas
        ''on va demander si l'utilisateur est bien sur de vouloir
        ''la supprimer pour la recréer
        ''les bases trouvées sont dans  data_base_list(1000, 10)

        Dim x As Integer = 0
        Dim Base_Name_tableau As String = ""

        For x = 1 To 1000
            Base_Name_tableau = data_base_list(x, 1)
            Base_Name_tableau = UCase(Base_Name_tableau)

            Nom_de_la_base = UCase(Nom_de_la_base)

            If (Base_Name_tableau = Nom_de_la_base) Then

                'Dim Retour = (MsgBox(Message_utilisateur(10, Form_Demarrage.Langue_logiciel) & """" & Nom_de_la_base & """" & Message_utilisateur(11, Form_Demarrage.Langue_logiciel), MsgBoxStyle.YesNo, Message_utilisateur(12, Form_Demarrage.Langue_logiciel)))



