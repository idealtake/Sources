    Public Function SupprimerConnexion(ByVal Nom_Connexion As String)

        If (Nom_Connexion.Length.ToString = "0") Then
            'il n'y a pas d'enregistrement
            'on fait rien
            'MsgBox("rien")

        Else
            'il y a un enregistrement à supprimer
            'MsgBox("Il y a un enregistrement sélectionné : " & String_Selection_connexion2)
            'Un tableau mémoire contiendra l'ensemble des données du fichier .xml
            'Public Liste_Connexions(100, 15) As String
            'Titre de la connexion, Adresse Serveur, Utilisateur, Mot de passe,
            'Confirmer le mot de passe, Nom de la base, chemin Schéma,c hemin synoptique
            'chemin notice, chemin fiche produit,chemin photos produit

            Dim Connexion_a_supprimer As String
            Connexion_a_supprimer = Nom_Connexion

            'Dim Connexion_a_supprimer As String = Nothing
            'Connexion_a_supprimer = Nom_Connexion

            'Dim Retour = (MsgBox(Message_utilisateur(21, Form_Demarrage.Langue_logiciel) & """" & Connexion_a_supprimer & """", MsgBoxStyle.YesNo, Message_utilisateur(12, Form_Demarrage.Langue_logiciel)))


            Dim Boite_dialogue As New IDEALTAKE.Message_Box_Question
            Boite_dialogue.Titre_Boite = MessageUtilisateur(12, Form_Demarrage.LangueLogiciel).ToString
            Boite_dialogue.Question_Boite = MessageUtilisateur(21, Form_Demarrage.LangueLogiciel).ToString & """" & Connexion_a_supprimer & """"
            Boite_dialogue.Oui_Boite = MessageUtilisateur(82, Form_Demarrage.LangueLogiciel).ToString
            Boite_dialogue.Non_Boite = MessageUtilisateur(83, Form_Demarrage.LangueLogiciel).ToString
            Boite_dialogue.ShowDialog()


            If (Boite_dialogue.Réponse_question = False) Then
                Return False
                Exit Function
            End If

            'If (Retour = vbNo) Then
            'Return False
            'Exit Function
            'End If

            Dim t As Integer
            Dim string_selection As String = ""
            Dim nmbre_de_connexion As Integer = 0
            Dim position_connexion As Integer = 0

            'string_selection = LTrim(Selection_connexion_ComboBox2.SelectedItem.ToString())
            'string_selection = RTrim(string_selection)

            string_selection = LTrim(Connexion_a_supprimer)
            string_selection = RTrim(Connexion_a_supprimer)


            For t = 1 To 100
                If (Len(Form_Demarrage.Liste_Connexions(t, 1)) > 0) Then
                    nmbre_de_connexion += 1
                End If

                If (Form_Demarrage.Liste_Connexions(t, 1) = string_selection) Then
                    position_connexion = t
                    'MsgBox(Selection_connexion_ComboBox2.SelectedItem.ToString & " : " & t)
                End If
            Next

            'MsgBox(Selection_connexion_ComboBox2.SelectedItem.ToString & " : " & position_connexion & "/" & nmbre_de_connexion)

            Dim settings As New XmlWriterSettings()
            Dim dossier As String = My.Application.Info.DirectoryPath & "\"
            'ici le fichier liste_des_connexions.xml va être réécrit

            settings.Encoding = System.Text.Encoding.UTF8
            settings.Indent = True
            settings.IndentChars = "    "
            Using writer As XmlWriter = XmlWriter.Create(dossier & "liste_des_connexions.xml", settings)
                ' Write XML data.
                writer.WriteStartElement("Liste_des_connexions")
                writer.WriteStartElement("Connexion")
                writer.WriteEndElement()

                For t = 1 To nmbre_de_connexion
                    If (t = position_connexion) Then
                        '' on fait rien
                        '' enregistrement qu'on supprime
                    Else
                        '' enregistrement qu'on conserve
                        writer.WriteStartElement("Connexion")
                        writer.WriteElementString("Nom_Connexion", Form_Demarrage.Liste_Connexions(t, 1))
                        writer.WriteElementString("Adresse_Serveur", Form_Demarrage.Liste_Connexions(t, 2))
                        writer.WriteElementString("Utilisateur", Form_Demarrage.Liste_Connexions(t, 3))
                        writer.WriteElementString("mot_de_passe", Form_Demarrage.Liste_Connexions(t, 4))
                        writer.WriteElementString("Nom_de_la_base", Form_Demarrage.Liste_Connexions(t, 5))
                        writer.WriteElementString("Description_Allemande", Form_Demarrage.Liste_Connexions(t, 6))
                        writer.WriteElementString("Description_Anglaise", Form_Demarrage.Liste_Connexions(t, 7))
                        writer.WriteElementString("Description_Espagnole", Form_Demarrage.Liste_Connexions(t, 8))
                        writer.WriteElementString("Description_Française", Form_Demarrage.Liste_Connexions(t, 9))
                        writer.WriteElementString("Description_Italienne", Form_Demarrage.Liste_Connexions(t, 10))
                        writer.WriteEndElement()
                    End If

                Next t

                writer.WriteEndElement()
                writer.Flush()

            End Using

            For t = 1 To 100
                Form_Demarrage.Liste_Connexions(t, 1) = ""   'on efface le nom de la connexion dans tout le tableau
            Next

            'Suppression de niveau d'acces pour cette connexion
            Dim chemin_fichier As String = ""
            chemin_fichier = My.Application.Info.DirectoryPath & "\niveau_access_" & Connexion_a_supprimer.ToString & ".txt"

            'tester la présence du fichier
            If File.Exists(chemin_fichier) Then
                'si il existe on le supprime
                Microsoft.VisualBasic.FileIO.FileSystem.DeleteFile(chemin_fichier)
            End If


            Form_Demarrage.Chargement_des_connexions()
            Me.Description_connexion_Label.Text = ""

            MsgBox(MessageUtilisateur(23, Form_Demarrage.LangueLogiciel) & """" & Connexion_a_supprimer & """ " & MessageUtilisateur(24, Form_Demarrage.LangueLogiciel), MsgBoxStyle.Information, MessageUtilisateur(22, Form_Demarrage.LangueLogiciel))

        End If


        Return True
    End Function





