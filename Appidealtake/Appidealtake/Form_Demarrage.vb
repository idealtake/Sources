    Private Function redim_table(ByVal chemin_complet_nom_répertoire As String, _
                                     ByVal Nom_du_xml As String, _
                                     ByVal nmbre_colonne As Integer, _
                                     ByRef DataSet_01 As DataSet, _
                                     ByRef Datatable_01 As DataTable, _
                                     ByVal langue_logiciel As Integer)


        Dim nbre_totale_enregistrement As Integer = 0
        'On s'occupe du Datatable

        If (System.IO.File.Exists(chemin_complet_nom_répertoire & "\" & Nom_du_xml) = True) Then
            'le fichier xml existe bien

            DataSet_01.ReadXml(chemin_complet_nom_répertoire & "\" & Nom_du_xml)

            If (DataSet_01.Tables.Count > 0) Then
                'Datatable_01.Clear()
                Datatable_01 = DataSet_01.Tables(0)                        'voila on a nos données filtré, trié 
                nbre_totale_enregistrement = Datatable_01.Rows.Count
            Else
                ' le fichier xml est vide
                nbre_totale_enregistrement = 1
            End If

        Else
            'Le fichier n'est xml n'est pas présent
            'Si c'est le cas une exception non géré 
            'va se produire d'ou -> nbre_totale_enregistrement = 1
            nbre_totale_enregistrement = 1
        End If



        Dim Données(nbre_totale_enregistrement - 1, nmbre_colonne - 1) As String
        Return Données
    End Function

    Function Log_MYSQL_VERS_XML(ByVal chemin_complet As String, ByVal Résultat As String)
        ' e.g ==> === Verbose logging started: 23/10/2010 


        Dim Log_file_stream As FileStream
        Dim Log_Stream_Writer As StreamWriter

        Dim verbose_logging_start As String = vbCrLf & vbTab & "=== Log Start : " & Date.Now.ToString & vbCrLf
        Dim verbose_logging_stop As String = vbCrLf & vbTab & "=== Log Stop" & vbCrLf
        Résultat = verbose_logging_start & Résultat & verbose_logging_stop



        'Test de la présence du répertoire
        If (Dir(chemin_complet, vbDirectory).Length < 1) Then
            'Le répertoire des données locales  n'est pas existant.

            MkDir(chemin_complet)
            'Le répertoire des données locales a été créé.

        End If

        chemin_complet = chemin_complet & "\" & "log.txt"

        If Not File.Exists(chemin_complet) Then
            Log_file_stream = File.Create(chemin_complet)
            Log_Stream_Writer = New StreamWriter(Log_file_stream)
            Log_Stream_Writer.WriteLine(Résultat)
            Log_Stream_Writer.Close()
        Else
            Log_Stream_Writer = File.AppendText(chemin_complet)
            Log_Stream_Writer.WriteLine(Résultat)
            Log_Stream_Writer.Close()

        End If

        Return True

    End Function

