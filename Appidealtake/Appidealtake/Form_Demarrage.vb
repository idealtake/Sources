            Next

        End If

        Return True
    End Function


    Function ChargementDesDonnéesEnregistrement()
        'MsgBox("Chargement des données de l'enregistrement")

        'AuthentificationGestionBase est le nom de l'utilisateur authentifié
        'Si la chaîne est vide alors on affiche la page de gestion des utilisateurs
        If (AuthentificationGestionBase.Length = 0) Then
            AffichageCompte_GestionBase()
        Else
            'La personne est bien authentifié, on poursuit.

            ' On vérifie qu'il y a bien un numéro de chrono qui
            ' a été sélectionné par l'utilisateur
            '
            '   AjoutEnregistrementChrono
            '   AjoutEnregistrementNumEnr


            If (AjoutEnregistrementChrono.Length <> 0) Then
                'ici on a tout un utilisateur authentifié et un numéro de chrono 
                ' On va lancer un requête qui va récupérer les infos provenants
                ' De la table 'enregistrement',  'chrono_idref'
                ' Puis on va les renvoyer dans la page HTML de modification de l'enregistrement


                Try

                    'Chargement de l'enregistrement en cours...
                    Dim document As System.Windows.Forms.HtmlDocument = WebBrowser_Gestion_Base.Document

                    'Etat_Ajout_Enregistrement
                    document.All("Etat_Ajout_Enregistrement").InnerText = Ajout_Suppression_connexion_base.MessageUtilisateur(281, LangueLogiciel)

                    'Etat_Ajout_Symbole
                    document.All("Etat_Ajout_Symbole").InnerText = Ajout_Suppression_connexion_base.MessageUtilisateur(281, LangueLogiciel)

                    'Etat_LiensReferences_Enregistrement
                    document.All("Etat_LiensReferences_Enregistrement").InnerText = Ajout_Suppression_connexion_base.MessageUtilisateur(281, LangueLogiciel)




                Catch ex As Exception

                End Try



                'ScoreChargementEnregistrement = 0    ==> Aucun chargement pour le moment
                'ScoreChargementEnregistrement = 1    ==> un  chargement sur deux  soit les données de la table 'enregistrement' ou les données de 'chrono_idref'
                'ScoreChargementEnregistrement = 2    ==> deux chargements sur deux  c'est fini ;)
                'My.Settings.ScoreChargementEnregistrement = 0


                'Ici on va lancer le chargement des données de l'enregistrement

                Dim ArgumentChargementEnregistrement As New Argument_ChargementEnregistrement


                ArgumentChargementEnregistrement.Auteur = AuthentificationGestionBase
                ArgumentChargementEnregistrement.chaine_de_connexion = connexion
                ArgumentChargementEnregistrement.Chrono = AjoutEnregistrementChrono
                ArgumentChargementEnregistrement.langue_logiciel = LangueLogiciel
                ArgumentChargementEnregistrement.Nom_de_la_base_de_données = Nom_de_la_base.Text



                If Not BackgroundWorkerChargementEnregistrement.IsBusy = True Then

                    TimerChargementEnregistrement.Start()

                    BackgroundWorkerChargementEnregistrement.RunWorkerAsync(ArgumentChargementEnregistrement)



                End If


            Else
                'pas de numéro de chrono sélectionné on fait rien

            End If

        End If





        Return True
    End Function



