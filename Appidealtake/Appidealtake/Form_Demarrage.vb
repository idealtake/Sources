    Private Function RECHERCHE_PAR_COMMENTAIRE_Table_SCHEMA(ByVal sender As System.Object, ByVal e As System.EventArgs, ByVal Commentaire As String)



        Dim Rch_Com_Str As String
        Dim strstandard1 As String
        Rch_Com_Str = UCase(Commentaire)
        Rch_Com_Str = LTrim(Rch_Com_Str)
        Rch_Com_Str = RTrim(Rch_Com_Str)
        Dim Liste_Reponse(5000, 35) As String                         'tableau contenant les données de la base
        Dim count As Integer                                          'indique le nombre de réponse
        Dim Date_Recherche As String
        Dim mySelectQuery As String = "SELECT * FROM " & Me.Nom_de_la_base.Text & ".schema"
        strstandard1 = ""
        Dim Nbre_NIVEAU_ACC As Integer = 0
        Dim T As Integer = 0

        If (Rch_Com_Str = "") Then
            MsgBox("Vous devez taper un commentaire pour faire une recherche.", MsgBoxStyle.Information, "Information")
            Return ""
        End If

        'empéche les erreurs de requête avec ' interdit le mot nul ou NUL
        If (InStr(Rch_Com_Str, "'") > 0) Then
            MsgBox("le caractère ' est interdit dans le commentaire recherché", MsgBoxStyle.Information, "Information")
            Return ""
        End If


        Try
            count = 0    'initialise le compteur d'occurence
            Date_Recherche = Date.Now  'Récupération de la date de recherche

            mySelectQuery = mySelectQuery & " WHERE (INSTR(COMMENTAIRE,'" & Rch_Com_Str & "'))"
            'MsgBox(mySelectQuery)



            If (Parametrage_REQUETE_Str = "AUCUN" Or Parametrage_REQUETE_Str = "0") Then
                ' Requête sans conditions aucune
            Else
                'Paramètre du niveau d'acces du client
                '    Public Parametrage_REQUETE_Str As String        'Niveau d'acces
                'Liste des niveau d'acces de la base
                '    Public Table_niveau_acces(100, 10)    'Niveau d'acces au document : Bas Niveau, Normal, confidentiel


                Dim position_Mon_Niveau_acces As Integer = 0

                For T = 1 To 100

                    If (Len(Table_niveau_acces(T, 1)) > 0) Then
                        Nbre_NIVEAU_ACC += 1   'Combien y a t-il de niveau d'acces?
                    End If

                    If (Table_niveau_acces(T, 3) = Parametrage_REQUETE_Str) Then
                        position_Mon_Niveau_acces = T   'Ou se positionne mon niveau d'acces?
                    End If

                Next


                If (InStr(mySelectQuery, "WHERE") > 0) Then
                    mySelectQuery = mySelectQuery & " and (NIVEAU_ACC = '" & Table_niveau_acces(1, 3) & "' "
                    For T = 2 To position_Mon_Niveau_acces
                        mySelectQuery = mySelectQuery & " or NIVEAU_ACC ='" & Table_niveau_acces(T, 3) & "' "
                    Next

                Else
                    mySelectQuery = mySelectQuery & " WHERE (NIVEAU_ACC = '" & Table_niveau_acces(1, 3) & "' "
                    For T = 2 To position_Mon_Niveau_acces
                        mySelectQuery = mySelectQuery & " or NIVEAU_ACC ='" & Table_niveau_acces(T, 3) & "' "
                    Next

                End If

                mySelectQuery = mySelectQuery & ") "

                'mySelectQuery = mySelectQuery & " and (NIVEAU_ACC = '" & Parametrage_REQUETE_Str & "' or NIVEAU_ACC ='" & Parametrage_REQUETE_Str & "') "
                'mySelectQuery = mySelectQuery & " WHERE (NIVEAU_ACC = '" & Parametrage_REQUETE_Str & "' or NIVEAU_ACC ='" & Parametrage_REQUETE_Str & "') "

            End If 'fin du if (Parametrage_REQUETE_Str = "AUCUN")

            mySelectQuery = mySelectQuery & "   ORDER BY NUM_SCHEMA ASC;"

            MsgBox(mySelectQuery)

            Dim myConnection As New MySqlConnection(connexion)
            Dim myCommand As New MySqlCommand(mySelectQuery, myConnection)
            myConnection.Open()

            Dim myReader As MySqlDataReader

            myReader = myCommand.ExecuteReader()


            While myReader.Read()

                Liste_Reponse(count, 0) = myReader.GetString(0) & "" 'Récupère le numéro de schéma
                'Liste_Reponse(count, 1) = myReader.GetString(1) & "" 'Récupère le numéro de Chrono
                Liste_Reponse(count, 2) = myReader.GetString(2) & "" 'Récupère le TYPE_DOC
                Liste_Reponse(count, 10) = myReader.GetString(10) & "" 'Récupère le commentaire
                Liste_Reponse(count, 12) = myReader.GetString(12) & "" 'Récupère la Référence 2
                Liste_Reponse(count, 13) = myReader.GetString(13) & "" 'Récupère la Référence 3
                Liste_Reponse(count, 14) = myReader.GetString(14) & "" 'Récupère la Référence 4
                Liste_Reponse(count, 15) = myReader.GetString(15) & "" 'Récupère la Référence 5
                Liste_Reponse(count, 16) = myReader.GetString(16) & "" 'Récupère la Référence 6
                Liste_Reponse(count, 17) = myReader.GetString(17) & "" 'Récupère la Référence 7
                Liste_Reponse(count, 18) = myReader.GetString(18) & "" 'Récupère la Référence 8
                Liste_Reponse(count, 19) = myReader.GetString(19) & "" 'Récupère la Référence 9
                Liste_Reponse(count, 20) = myReader.GetString(20) & "" 'Récupère la Référence 10
                Liste_Reponse(count, 32) = myReader.GetString(32) & "" 'Récupère le niveau d'accés

                count = count + 1                               ' compte le nombre d'occurence



            End While

            ' always call Close when done reading.

            myReader.Close()

            ' Close the connection when done with it.

            myConnection.Close()
            'ancienne valeur "00ffbb" remplace par Couleur_fond_résultat_RF_COM
            Dim Texte_à_dire As String = ""
            If (My.Settings.Activation_Synthese_Vocale = True) Then
                Texte_à_dire = Type_de_recherche("Simplifié") 'synthèse vocale du type de recherche
            End If

            'on rend visible le menu Zoom disponible
            'MenuStrip1.Visible = True
            'Affiche_html(WebBrowser1, Couleur_fond_résultat_Recherche_simplifiée, count, Date_Recherche, Liste_Reponse, "\rech_commentaire.htm", " Par le commentaire du Sch&eacute;ma", Texte_à_dire)

            ''affiche la requête SQL
            ''MsgBox(mySelectQuery)



        Catch ex As Exception
            If (InStr(ex.Message.ToString, "404") > 0) Then
            Else
                MessageBox.Show(ex.Message, "Erreur_1 RECHERCHE_PAR_COMMENTAIRE_Table_SCHEMA()", MessageBoxButtons.OK, MessageBoxIcon.Error)
            End If
            Return ""
        End Try

        Return Liste_Reponse
    End Function


