    Function BR_45C(ByVal Chaine_entree As String)

        Dim split As String()
        Dim counter2 As Integer = 1
        Dim counter1 As Integer = 1
        Dim old_chaine As String = ""
        Dim Chaine_sortie As String = ""
        Dim flag As Boolean = True

        split = Chaine_entree.Split(" ")

        Dim s As String
        For Each s In split
            If (Microsoft.VisualBasic.Strings.Len(s) > 0) Then
                counter2 = counter2 + 1
            End If
        Next s

        Dim str_45(counter2 + 1) As String

        For Each s In split

            If (Microsoft.VisualBasic.Strings.Len(old_chaine & s & " ") > 40) Then
                str_45(counter1) = old_chaine
                old_chaine = s
                counter1 = counter1 + 1

            Else
                If (old_chaine = "") Then
                    old_chaine = s
                Else
                    old_chaine = old_chaine & " " & s
                End If

            End If

        Next s


        If ((Microsoft.VisualBasic.Strings.Len(old_chaine) < 41) And (counter1 = 1)) Then
            str_45(0) = old_chaine

        End If


        If ((Microsoft.VisualBasic.Strings.Len(old_chaine) < 41) And (counter1 > 1)) Then
            str_45(counter1) = old_chaine

        End If

        For Each s In str_45
            If (Microsoft.VisualBasic.Strings.Len(s) > 0) Then
                Chaine_sortie = Chaine_sortie & "&nbsp;&nbsp;" & s & "<br />"
            End If

        Next s



        Return Chaine_sortie

    End Function



