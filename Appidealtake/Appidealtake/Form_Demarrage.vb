        Dim p
        liste = GetObject("winmgmts:").InstancesOf("Win32_baseboard") ' Win32_baseboard a remplacer par l'instance desirée

        For Each p In liste
            'Constructeur
            Me.Texte_Aide.Text = Me.Texte_Aide.Text & "  " & Ajout_Suppression_connexion_base.MessageUtilisateur(72, LangueLogiciel) & " : " & p.manufacturer & vbCrLf

