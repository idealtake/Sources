    Function Vérification_de_la_présence_du_fichier_commandes_SAPI5()



        Dim Lignes_Commandes_SAPI5_String As String = ""
        Dim Flux_Fichier As FileStream
        Dim Flux_écriture As StreamWriter
        Dim chemin_commandes_SAPI5 As String = ""

        ' Configuration synthèse vocale par un fichier d'échange 
        ' commandes_SAPI5.txt
        ' si il n'existe pas on le créé.

        chemin_commandes_SAPI5 = My.Application.Info.DirectoryPath & "\commandes_SAPI5.txt"
        Lignes_Commandes_SAPI5_String = "LANGUE = " & Me.LangueLogiciel & vbCrLf & _
                                        "VISIBILITE_DU_LOGICIEL = Oui" & vbCrLf & _
                                        "TEXTE_A_DIRE = """"" & vbCrLf & _
                                        "EXECUTION_DE_LA_SYNTHESE_VOCALE = Non" & vbCrLf & _
                                        "ETAT_DE_FONCTIONNEMENT_SAPI = Non" & vbCrLf & _
                                        "TEMPS_DU_TIMER = 800"

        If Not File.Exists(chemin_commandes_SAPI5) Then

            Flux_Fichier = File.Create(chemin_commandes_SAPI5)
            Flux_écriture = New StreamWriter(Flux_Fichier)
            Flux_écriture.WriteLine(Lignes_Commandes_SAPI5_String)
            Flux_écriture.Close()

        End If
        Return True
    End Function

