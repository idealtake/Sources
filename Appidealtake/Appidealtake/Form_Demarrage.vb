    Private Function Création_corp_page_html(ByVal Langue_Logiciel As Integer, ByVal Table_éléments_HTML As DataTable, ByVal Numéro_de_page As Integer)

        'exemple d'enregistrement dans le fichier elements_HTML.xml
        '    <elements_HTML>
        '     <ID_elements_HTML>19</ID_elements_HTML>
        '     <Name_of_page>Guided_Research</Name_of_page>
        '     <Additional_Name>---</Additional_Name>
        '     <Position_in_page>5</Position_in_page>
        '     <Name_of_TAG><HTML></Name_of_TAG>
        '     <German_CODE>GD_A5</German_CODE>
        '     <English_CODE>GD_B5</English_CODE>
        '     <Spanish_CODE>GD_C5</Spanish_CODE>
        '     <French_CODE>GD_D5</French_CODE>
        '     <Italian_CODE>GD_E5</Italian_CODE> 
        '    </elements_HTML>



        Dim Table_Temp As DataTable
        Table_Temp = Table_éléments_HTML
        Dim page_modele_HTML As String = ""
        Dim Nom_de_la_page As String = ""

        Select Case Numéro_de_page
            Case 0 ' Simplified_Research
                Nom_de_la_page = "Simplified_Research"
            Case 1 ' Advanced_Research
                Nom_de_la_page = "Advanced_Research"
            Case 2 ' Guided_Research
                Nom_de_la_page = "Guided_Research"
            Case 3 ' Record_Detail
                Nom_de_la_page = "Record_Detail"
            Case 4 ' Data_Managment
                Nom_de_la_page = "Data_Managment"
            Case 5 ' Find_Data
                Nom_de_la_page = "Find_Data"
            Case 6 ' Add_Data
                Nom_de_la_page = "Add_Data"
            Case 7 ' Modify_Data
                Nom_de_la_page = "Modify_Data"
            Case 8 ' Delete_Data
                Nom_de_la_page = "Delete_Data"
            Case 9 ' User_Management
                Nom_de_la_page = "User_Management"
        End Select


        Try


            'Dim table As DataTable = DataSet1.Tables(0)
            ' Presuming the DataTable has a column named Date.
            'Dim expression As String = "Date > 1/1/00"
            ' Sort descending by column named CompanyName.
            'Dim sortOrder As String = "CompanyName DESC"
            'Dim foundRows() As DataRow
            ' Use the Select method to find all rows matching the filter.
            'foundRows = table.Select(expression, sortOrder)
            ' Dim i As Integer
            ' Print column 0 of each returned row.
            ' For i = 0 To foundRows.GetUpperBound(0)
            'Console.WriteLine(foundRows(i)(0))
            ' Next i


            ' On présume que le DataTable dispose d'une colonne 'Name_of_page'
            Dim expression As String = "Name_of_page = '" & Nom_de_la_page & "'"

            ' 14/09/2012
            ' on déscative le sortOrder de position_in_page qui est un string 
            ' ce trie ne permet pas d'avoir plus de 9 lignes par page
            ' On définit une nouvelle colonne qui contient cette fois ci un entier

            'Trie Ascendant par la colonne 'Position_in_page'
            'Dim sortOrder As String = "Position_in_page ASC"


            'ici on ajoute une nouvelle colonne   "IntegerPositionInPage"
            'Qui va contenir la chaine de la colonne "Position_in_page"  convertit en integer


            If (Table_Temp.Columns.Contains("IntegerPositionInPage")) Then 'si la colonne IntegerPositionInPage n'existe pas alors

                ' la colonne existe déja
            Else

                'la colonne n'existe pas

                ' Declare variables for DataColumn and DataRow objects.
                Dim column As DataColumn

                ' Create new DataColumn, set DataType, ColumnName 
                ' and add to DataTable.    
                column = New DataColumn()
                column.DataType = System.Type.GetType("System.Int32")
                column.ColumnName = "IntegerPositionInPage"
                'column.ReadOnly = True
                'column.Unique = True

                ' Add the Column to the DataColumnCollection.
                Table_Temp.Columns.Add(column)
                'MsgBox(Table_Temp.Columns.Item("IntegerPositionInPage").ToString)
            End If

            'Dim i0 As Integer
            Dim TempInt0 As Integer = 0
            Dim ligne_de_données0 As DataRow
            ' For i0 = 0 To Table_Temp.Rows.Count
            '  Next

            ' On copie la valeur de Position_in_page qui est une chaine
            ' dans la colonne IntegerPositionInPage qui est un integer 
            ' utilisant   CIn()

            For Each ligne_de_données0 In Table_Temp.Rows
                TempInt0 = CInt(ligne_de_données0.Item("Position_in_page"))
                ligne_de_données0.Item("IntegerPositionInPage") = TempInt0
                TempInt0 = 0
            Next

            ' on définit le sort sur la colonne  IntegerPositionInPage  qui est un entier
            Dim sortOrder As String = "IntegerPositionInPage ASC"


            Dim foundRows() As DataRow
            'Utilisation de la méthode Select() afin de trouver toutes les lignes répondants au filtre
            foundRows = Table_Temp.Select(expression, sortOrder)

            Dim i As Integer

            'retourne la colonne Langue_CODE pour chacune des lignes
            For i = 0 To foundRows.GetUpperBound(0)
                'Console.WriteLine(foundRows(i)(0))


                Select Case Langue_Logiciel
                    Case 0 ' Allemand
                        page_modele_HTML += foundRows(i)("German_CODE")
                    Case 1 ' Anglais
                        page_modele_HTML += foundRows(i)("English_CODE")
                    Case 2 ' Espagnol
                        page_modele_HTML += foundRows(i)("Spanish_CODE")
                    Case 3 ' Français
                        page_modele_HTML += foundRows(i)("French_CODE")
                    Case 4 ' Italien
                        page_modele_HTML += foundRows(i)("Italian_CODE")
                End Select

            Next i

        Catch ex As Exception
            Return page_modele_HTML
        End Try

        Return page_modele_HTML
    End Function


