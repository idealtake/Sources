    Public Function Transformation_Date(ByVal Str_Date_de_reponse As String)
        Dim T As Integer = 1
        Dim counter_SLASH As Integer = 0
        Dim Position_Slash_1 As Integer = 0
        Dim Position_Slash_2 As Integer = 0

        For T = 1 To Str_Date_de_reponse.Length
            Select Case Microsoft.VisualBasic.Strings.GetChar(Str_Date_de_reponse, T)
                Case "/"
                    counter_SLASH += 1

                    If (counter_SLASH = 1) Then
                        Position_Slash_1 = T  'récupère la position de la première occurence de slash
                    End If

                    If (counter_SLASH = 2) Then
                        Position_Slash_2 = T  'récupère la position de la deuxième occurence de slash
                    End If

            End Select

        Next T

        Dim Jour_Date As String = ""
        Dim Mois_Date As String = ""
        Dim Annee_Date As String = ""

        Dim Heure_Date As String = ""
        Dim Minute_Date As String = ""
        Dim Seconde_Date As String = ""
        Dim Str_Date_Transformée As String = ""

        'MsgBox(Str_Date_de_reponse)
        Jour_Date = Mid(Str_Date_de_reponse, 1, Position_Slash_1 - 1)
        Mois_Date = Mid(Str_Date_de_reponse, (Position_Slash_1 + 1), ((Position_Slash_2 - 1) - Position_Slash_1))
        'Annee_Date = Mid(Str_Date_de_reponse, (Position_Slash_2 + 1), (Str_Date_de_reponse.Length) - Position_Slash_2)
        Annee_Date = Mid(Str_Date_de_reponse, (Position_Slash_2 + 1), 4)

        Heure_Date = Mid(Str_Date_de_reponse, (Position_Slash_2 + 6), 2)
        Minute_Date = Mid(Str_Date_de_reponse, (Position_Slash_2 + 9), 2)
        Seconde_Date = Mid(Str_Date_de_reponse, (Position_Slash_2 + 12), 2)

        Str_Date_Transformée = Annee_Date & "-" & Mois_Date & "-" & Jour_Date & " " & Heure_Date & ":" & Minute_Date & ":" & Seconde_Date

        Return Str_Date_Transformée
    End Function

