    Public Function Chargement_du_context_menu(ByVal Table_constructeurs As Object, _
                                                   ByVal Table_catégories As Object, _
                                                   ByVal Table_Références As Object, _
                                                   ByRef Context_menu As Object, _
                                                   ByVal Langue As Integer, _
                                                   ByVal sub_name As String)
        Context_menu.Items.Clear()
        If Table_constructeurs.length = 0 Then Return True
        Dim nombre_enregistrement_Table_constructeurs As Integer = 0
        nombre_enregistrement_Table_constructeurs = Table_constructeurs.length / 12
        Dim élément_menu_outil_Constructeurs As ToolStripMenuItem() = New ToolStripMenuItem(nombre_enregistrement_Table_constructeurs - 1) {}

        If Table_catégories.length = 0 Then Return True
        Dim nombre_enregistrement_Table_catégories As Integer = 0
        nombre_enregistrement_Table_catégories = Table_catégories.length / 7
        Dim élément_menu_outil_Categorie As ToolStripMenuItem() = New ToolStripMenuItem(nombre_enregistrement_Table_catégories - 1) {}

        If Table_Références.length = 0 Then Return True
        Dim nombre_enregistrement_Table_Références As Integer = 0
        nombre_enregistrement_Table_Références = Table_Références.length / 10
        'Dim élément_menu_outil_Reference As ToolStripComboBox() = New ToolStripComboBox(nombre_enregistrement_Table_Références - 1) {}
        'il y a autant de combobox qu'il n'existe de catégories
        Dim élément_menu_outil_Reference As ToolStripComboBox() = New ToolStripComboBox(nombre_enregistrement_Table_catégories - 1) {}

        'Ajout des constructeurs dans élément_menu_outil_Constructeurs
        'élément_menu_outil_Constructeurs(0) = New System.Windows.Forms.ToolStripMenuItem
        'élément_menu_outil_Constructeurs(0).Name = "Constructeur" & 1
        'élément_menu_outil_Constructeurs(0).Text = "URMET"
        Dim T As Integer
        For T = 0 To nombre_enregistrement_Table_constructeurs - 1
            élément_menu_outil_Constructeurs(T) = New System.Windows.Forms.ToolStripMenuItem
            élément_menu_outil_Constructeurs(T).Name = "Constructeur_" & T
            élément_menu_outil_Constructeurs(T).Text = Table_constructeurs(T, 1)
        Next


        'Ajout des constructeurs dans le context menu
        'user.Liste_des_références_Menu_Strip.Items.Add(élément_menu_outil_Constructeurs(0))
        For T = 0 To nombre_enregistrement_Table_constructeurs - 1
            Context_menu.Items.Add(élément_menu_outil_Constructeurs(T))
        Next


        'Ajout des catégories dans élément_menu_outil_Categorie
        'élément_menu_outil_Categorie(0) = New System.Windows.Forms.ToolStripMenuItem
        'élément_menu_outil_Categorie(0).Name = "Categorie" & 1
        'élément_menu_outil_Categorie(0).Text = "Alimentation"
        For T = 0 To nombre_enregistrement_Table_catégories - 1
            élément_menu_outil_Categorie(T) = New System.Windows.Forms.ToolStripMenuItem
            élément_menu_outil_Categorie(T).Name = "Categorie_" & T

            Select Case Langue
                Case 0 'Allemand
                    élément_menu_outil_Categorie(T).Text = Table_catégories(T, 2)
                Case 1 'Anglais
                    élément_menu_outil_Categorie(T).Text = Table_catégories(T, 3)
                Case 2 'Espagnole
                    élément_menu_outil_Categorie(T).Text = Table_catégories(T, 4)
                Case 3 'Français
                    élément_menu_outil_Categorie(T).Text = Table_catégories(T, 5)
                Case 4 'Italien
                    élément_menu_outil_Categorie(T).Text = Table_catégories(T, 6)

            End Select

        Next


        'Ajout des catégories aux différents constructeurs
        'élément_menu_outil_Constructeurs(0).DropDownItems.Add(élément_menu_outil_Categorie(0))
        'élément_menu_outil_Constructeurs(0).DropDownItems.Add(élément_menu_outil_Categorie(1))
        For T = 0 To nombre_enregistrement_Table_constructeurs - 1
            Dim J As Integer
            For J = 0 To nombre_enregistrement_Table_catégories - 1

                Select Case Langue
                    Case 0 'Allemand
                        If ((élément_menu_outil_Categorie(J).Text = Table_catégories(J, 2)) And _
                            (Table_constructeurs(T, 0) = Table_catégories(J, 1))) Then
                            élément_menu_outil_Constructeurs(T).DropDownItems.Add(élément_menu_outil_Categorie(J))
                        End If

                    Case 1 'Anglais
                        If ((élément_menu_outil_Categorie(J).Text = Table_catégories(J, 3)) And _
                            (Table_constructeurs(T, 0) = Table_catégories(J, 1))) Then
                            élément_menu_outil_Constructeurs(T).DropDownItems.Add(élément_menu_outil_Categorie(J))
                        End If

                    Case 2 'Espagnole
                        If ((élément_menu_outil_Categorie(J).Text = Table_catégories(J, 4)) And _
                            (Table_constructeurs(T, 0) = Table_catégories(J, 1))) Then
                            élément_menu_outil_Constructeurs(T).DropDownItems.Add(élément_menu_outil_Categorie(J))
                        End If

                    Case 3 'Français
                        If ((élément_menu_outil_Categorie(J).Text = Table_catégories(J, 5)) And _
                            (Table_constructeurs(T, 0) = Table_catégories(J, 1))) Then
                            élément_menu_outil_Constructeurs(T).DropDownItems.Add(élément_menu_outil_Categorie(J))
                        End If

                    Case 4 'Italien
                        If ((élément_menu_outil_Categorie(J).Text = Table_catégories(J, 6)) And _
                            (Table_constructeurs(T, 0) = Table_catégories(J, 1))) Then
                            élément_menu_outil_Constructeurs(T).DropDownItems.Add(élément_menu_outil_Categorie(J))
                        End If

                End Select

            Next

        Next



        'Ajout des références dans élément_menu_outil_Reference
        'élément_menu_outil_Reference(0) = New System.Windows.Forms.ToolStripComboBox
        'élément_menu_outil_Reference(0).Name = "Reference" & 1
        'élément_menu_outil_Reference(0).ComboBox.Items.Add("9000/230")
        'élément_menu_outil_Reference(0).AutoCompleteMode = System.Windows.Forms.AutoCompleteMode.SuggestAppend
        'élément_menu_outil_Reference(0).AutoCompleteSource = System.Windows.Forms.AutoCompleteSource.ListItems
        'AddHandler élément_menu_outil_Reference(0).SelectedIndexChanged, AddressOf RA_ajoute_une_reference_à_la_recherche
        Dim U, V As Integer
        Dim Idconstructeur, Idcategorie As Integer
        Dim id_catégorie_de_la_réference As Integer
        Dim id_constructeur_de_la_réference As Integer
        Idconstructeur = Idcategorie = id_catégorie_de_la_réference = id_constructeur_de_la_réference = 0
        Dim Compteur As Integer = 0

        For T = 0 To nombre_enregistrement_Table_constructeurs - 1
            Idconstructeur = Table_constructeurs(T, 0)


            For U = 0 To nombre_enregistrement_Table_catégories - 1 'pour chaque catégorie il y a un combobox

                Compteur = Compteur + 1

                Idcategorie = Table_catégories(U, 0)

                élément_menu_outil_Reference(U) = New System.Windows.Forms.ToolStripComboBox
                élément_menu_outil_Reference(U).Name = "Reference_" & Compteur

                For V = 0 To nombre_enregistrement_Table_Références - 1

                    id_catégorie_de_la_réference = CInt(Table_Références(V, 8))
                    id_constructeur_de_la_réference = CInt(Table_Références(V, 1))

                    'MsgBox("Const." & Idconstructeur & "; Cat." & Idcategorie & vbCrLf _
                    '      & Table_Références(V, 7) & vbCrLf _
                    '      & "Const." & id_constructeur_de_la_réference _
                    '      & "Cat." & id_catégorie_de_la_réference & vbCrLf _
                    '      )
                    'If ((id_constructeur_de_la_réference = Idconstructeur) And _
                    '(id_catégorie_de_la_réference = Idcategorie)) Then

                    If (id_catégorie_de_la_réference = Idcategorie) Then
                        Dim temp0 As String
                        temp0 = Table_Références(V, 7)
                        temp0 = LTrim(temp0)
                        temp0 = RTrim(temp0) '& "|" & id_constructeur_de_la_réference & "_" & id_catégorie_de_la_réference

                        élément_menu_outil_Reference(U).ComboBox.Items.Add(temp0)
                        'MsgBox(Table_Références(V, 7))
                        'MsgBox("Marche!")

                    End If



                    'élément_menu_outil_Reference(U).ComboBox.Items.Add("ALPHA" & U)
                Next

                élément_menu_outil_Reference(U).AutoCompleteMode = System.Windows.Forms.AutoCompleteMode.SuggestAppend
                élément_menu_outil_Reference(U).AutoCompleteSource = System.Windows.Forms.AutoCompleteSource.ListItems

                If (sub_name = "RA_ajoute_une_reference_à_la_recherche") Then
                    AddHandler élément_menu_outil_Reference(U).SelectedIndexChanged, AddressOf RA_ajoute_une_reference_à_la_recherche
                End If


                If (sub_name = "RS_ajoute_une_reference") Then
                    AddHandler élément_menu_outil_Reference(U).SelectedIndexChanged, AddressOf RS_ajoute_une_reference
                End If

            Next

        Next

        'Ajout des références dans les catégories
        'élément_menu_outil_Categorie(0).DropDownItems.Add(élément_menu_outil_Reference(0))
        'élément_menu_outil_Categorie(1).DropDownItems.Add(élément_menu_outil_Reference(1))

        For U = 0 To (nombre_enregistrement_Table_catégories - 1) 'pour chaque catégorie il y a un combobox  
            élément_menu_outil_Categorie(U).DropDownItems.Add(élément_menu_outil_Reference(U))
        Next
        'élément_menu_outil_Categorie(0).DropDownItems.Add(élément_menu_outil_Reference(0))

        Return True

    End Function



