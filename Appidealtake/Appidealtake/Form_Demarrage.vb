    Private Function Backup_Table_en_xml(ByVal nom_table As String, ByVal chemin_et_nom_Fichier As String, _
                                  ByVal nbre_colonne As Integer, ByVal Liste_colonne(,) As Object, _
                                  ByVal Nbre_réponse As Integer, ByVal Liste_Reponse(,) As Object)

        Dim settings As New XmlWriterSettings()

        Using writer As XmlWriter = XmlWriter.Create(chemin_et_nom_Fichier, settings)
            writer.WriteStartElement(nom_table)

            Dim Bcl1 As Integer
            Dim blc2 As Integer
            For Bcl1 = 0 To (Nbre_réponse - 1)
                writer.WriteStartElement("DATA")

                For blc2 = 0 To (nbre_colonne - 1)
                    writer.WriteElementString(Liste_colonne(blc2, 0).ToString, Liste_Reponse(Bcl1, blc2))
                Next

                writer.WriteEndElement()
            Next
            writer.WriteEndElement()
            writer.Flush()

        End Using
        Return True
    End Function

