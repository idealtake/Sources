    Private Function RequêteSQLAjoutLiensRéférences(ByVal chrono As String, ByVal Auteur As String, ByVal LIsteRéférences As List(Of Liste_Références), ByVal chaine_de_connexion As String, ByVal Nom_de_la_base_de_données As String, ByVal langue_logiciel As Integer)


        Dim Retour As New RetourAjoutLiensRéférences
        Dim Infos_sur_la_requête_AjoutLiensRéférences As String = ""
        Dim hostNameOrAddress As String = "localhost"
        Dim returnValue As IPHostEntry
        Dim AdresseIP As String = ""
        Dim RetourEcritureLog As New RetourLog

        returnValue = Dns.GetHostEntry(hostNameOrAddress)


        ' Nom de la machine
        Dim NomMachine As String = Dns.GetHostName

        ' Récupération de la liste des IP de la machine
        Dim InfoIps As IPHostEntry = Dns.GetHostEntry(NomMachine)
        Dim MesIp As IPAddress() = InfoIps.AddressList


        For Each CurrentIp As IPAddress In MesIp
            AdresseIP += CurrentIp.ToString & "; "
        Next

        If AdresseIP.Length > 254 Then AdresseIP = AdresseIP.Substring(0, 254)

        'e.g : Dim requete_SupprimerEnregistrement As String = "DELETE FROM  " & NomDeLaBaseDeDonnées & ".enregistrements WHERE   Auteur = '" & Utilisateur & "'  AND Chrono = " & chrono & " ;"

        Try

            Dim Mysql_connection As New MySql.Data.MySqlClient.MySqlConnection(chaine_de_connexion)
            Mysql_connection.Open()

            ' Phase 1 - Suppression des liens existants
            ' Syntaxe Delete
            ' DELETE [LOW_PRIORITY] [QUICK]   
            ' FROM(table_name)
            ' [WHERE conditions] [ORDER BY ...] [LIMIT rows]

            Dim requete_SupprimerLiensRéférences As String = "DELETE FROM  " & Nom_de_la_base_de_données & ".chrono_idref WHERE  Chrono = " & chrono & " ;"


            Infos_sur_la_requête_AjoutLiensRéférences += "Requête : " & requete_SupprimerLiensRéférences & vbCrLf

            Dim Commande_SupprimerLiensRéférences As New MySql.Data.MySqlClient.MySqlCommand

            Commande_SupprimerLiensRéférences.Connection = Mysql_connection
            Commande_SupprimerLiensRéférences.CommandText = requete_SupprimerLiensRéférences

            Dim NbreLignes As Integer = 0

            NbreLignes = Commande_SupprimerLiensRéférences.ExecuteNonQuery() 'Execution de la requête 

            If NbreLignes = 1 Then
                Infos_sur_la_requête_AjoutLiensRéférences += "Suppression Liens Références, Chrono[" & chrono & "]. OK" & vbCrLf
            Else
                Infos_sur_la_requête_AjoutLiensRéférences += "Suppression Liens Références, Chrono[" & chrono & "]. BAD" & vbCrLf
            End If

            Commande_SupprimerLiensRéférences.Dispose()

            Dim RequêteRécupérationUserID As String = ""
            RequêteRécupérationUserID = "SELECT User_ID FROM " & Nom_de_la_base_de_données & ".Users WHERE  User_Name = '" & Auteur & "';"
            Infos_sur_la_requête_AjoutLiensRéférences += vbCrLf & " RequêteSQLAjoutLiensRéférences() - RequêteRécupérationUserID" & vbCrLf
            Infos_sur_la_requête_AjoutLiensRéférences += RequêteRécupérationUserID & vbCrLf

            Dim myCommand1 As New MySql.Data.MySqlClient.MySqlCommand(RequêteRécupérationUserID, Mysql_connection)
            Dim myReader1 As MySql.Data.MySqlClient.MySqlDataReader
            myReader1 = myCommand1.ExecuteReader()

            Dim User_ID_Utilisateur As String = ""

            myReader1.Read()
            User_ID_Utilisateur = myReader1.GetInt16(0)

            myReader1.Close()
            myCommand1.Dispose()

            'Phase 2 - On créé un Log de la suppression des liens chrono, Références

            RetourEcritureLog = RequêteSQLLog(User_ID_Utilisateur, AdresseIP, returnValue.HostName, My.Computer.Info.OSFullName, _
                                              "Delete", "chrono_idref", chrono, requete_SupprimerLiensRéférences, "1", chaine_de_connexion, Nom_de_la_base_de_données)

            Infos_sur_la_requête_AjoutLiensRéférences += RetourEcritureLog.Infos_requêtes & vbCrLf


            'Phase 3 - On insère les données dans la table  "chrono_idref"

            ' INSERT INTO table_name ( field1, field2,...fieldN )
            ' VALUES                 ( value1, value2,...valueN ),( value1, value2,...valueN );


            Dim RequêteAjoutLiensRéférences As String = "INSERT INTO " & Nom_de_la_base_de_données & ".chrono_idref(Chrono,IDRef) VALUES"
            Dim i1 As Integer = 0

            For i1 = 0 To (LIsteRéférences.Count - 1)
                RequêteAjoutLiensRéférences += "(" & chrono & "," & LIsteRéférences.Item(i1).IDRef & ")"
                If i1 < (LIsteRéférences.Count - 1) Then RequêteAjoutLiensRéférences += ","

            Next

            RequêteAjoutLiensRéférences += ";"



            Infos_sur_la_requête_AjoutLiensRéférences += "Requête : " & RequêteAjoutLiensRéférences & vbCrLf

            Dim Commande_AjouterLiensRéférences As New MySql.Data.MySqlClient.MySqlCommand

            Commande_AjouterLiensRéférences.Connection = Mysql_connection
            Commande_AjouterLiensRéférences.CommandText = RequêteAjoutLiensRéférences

            NbreLignes = 0

            NbreLignes = Commande_AjouterLiensRéférences.ExecuteNonQuery() 'Execution de la requête 

            If NbreLignes = 1 Then
                Infos_sur_la_requête_AjoutLiensRéférences += "Ajout Liens Références, Chrono[" & chrono & "]. OK" & vbCrLf
            Else
                Infos_sur_la_requête_AjoutLiensRéférences += "Ajout Liens Références, Chrono[" & chrono & "]. BAD" & vbCrLf
            End If

            'Phase 4 - On créé un Log de l'ajout des liens chrono, Références

            RetourEcritureLog = RequêteSQLLog(User_ID_Utilisateur, AdresseIP, returnValue.HostName, My.Computer.Info.OSFullName, _
                                              "Add", "chrono_idref", chrono, RequêteAjoutLiensRéférences, "1", chaine_de_connexion, Nom_de_la_base_de_données)

            Infos_sur_la_requête_AjoutLiensRéférences += RetourEcritureLog.Infos_requêtes & vbCrLf


            Commande_AjouterLiensRéférences.Dispose()

            ' on confirme que tout c'est bien déroulé en renvoyant le numéro de chrono
            Retour.NumChrono = chrono

fin:        Mysql_connection.Close()
            Infos_sur_la_requête_AjoutLiensRéférences += vbCrLf & "Déconnexion." & vbCrLf

        Catch ex As MySql.Data.MySqlClient.MySqlException
            'MessageBox.Show(ex.ErrorCode & " : " & ex.Message, "Requêtes_SQL_Recherche_simplifiée() - Error - ")
            Infos_sur_la_requête_AjoutLiensRéférences += " : " & ex.Message & " RequêteSQLAjoutLiensRéférences() - Error - " & vbCrLf

            'test si la connection mysql est toujours ouverte
            'If Mysql_connection.State = ConnectionState.Open Then
            'si oui on la ferme
            'Mysql_connection.Close()
            'End If

            ' on confirme qu'un problème s'est produit avec un numéro de Chrono Vierge
            Retour.NumChrono = ""


            GoTo erreur_fin
        End Try
        Retour.Erreur = False
        Retour.Infos_requêtes = Infos_sur_la_requête_AjoutLiensRéférences

        Return Retour

erreur_fin:
        Retour.Erreur = True
        Retour.Infos_requêtes = Infos_sur_la_requête_AjoutLiensRéférences
        Return Retour


    End Function

