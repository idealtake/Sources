    Private Sub CheckBox_Activation_synthese_vocale_Click(ByVal sender As Object, ByVal e As System.EventArgs) Handles CheckBox_Activation_synthese_vocale.Click

        If (My.Settings.Modification_Objet = True) Then
            If (FonctionModificationObjet(CheckBox_Activation_synthese_vocale, sender.ToString) = True) Then
                Exit Sub
            End If
        End If

        'on désactive la case à cocher "CheckBox_Activation_synthese_vocale"
        Me.CheckBox_Activation_synthese_vocale.Enabled = False

        Vérification_de_la_présence_du_fichier_commandes_SAPI5()


        If (Me.CheckBox_Activation_synthese_vocale.Checked = True) Then


            Try

                My.Settings.Activation_Synthese_Vocale = True
                activation_Synthese_vocale(True)

                'on rend le logiciel non visible
                Me.modification_commandes_SAPI5_VISIBILITE_DU_LOGICIEL(False, My.Application.Info.DirectoryPath)

                'on passe l'interval du timer à une très faible valeur
                Me.modificationCommandesSapi5TempsDuTimer(10, My.Application.Info.DirectoryPath)

            Catch ex As Exception
                My.Settings.Activation_Synthese_Vocale = False
                activation_Synthese_vocale(False)
                Me.CheckBox_Activation_synthese_vocale.Checked = False
            End Try

            'on vérifie que le logiciel SAPI n'est pas déjà en cours d'execution
            If (consultation_état_ETAT_DE_FONCTIONNEMENT_SAPI(My.Application.Info.DirectoryPath) = False And SAPI5_en_cours_execution = False) Then

                Try
                    'Puis on lance l'execution de SAPI5.exe
                    'System.Diagnostics.Process.Start(My.Application.Info.DirectoryPath & "\SAPI5.exe")

                    If (Tâche_de_fond_SAPI.IsBusy = False) Then
                        Me.Tâche_de_fond_SAPI.WorkerSupportsCancellation = True
                        Me.Tâche_de_fond_SAPI.RunWorkerAsync()

                    Else
                        'ici le background worker et occupé
                        Me.Tâche_de_fond_SAPI.CancelAsync()
                        Me.Tâche_de_fond_SAPI.RunWorkerAsync()

                    End If


                    Me.SAPI5_en_cours_execution = True

                    'on indique qu'il est en fonctionnement
                    Me.modification_commandes_SAPI5_ETAT_DE_FONCTIONNEMENT_SAPI(True, My.Application.Info.DirectoryPath)




                Catch ex As Exception

                    ShowErrMsg("CheckBox_Activation_synthese_vocale_CheckedChanged()")
                    Me.SAPI5_en_cours_execution = False

                    My.Settings.Activation_Synthese_Vocale = False
                    activation_Synthese_vocale(False)
                    Me.CheckBox_Activation_synthese_vocale.Checked = False

                    'arrêt de la tâche
                    Me.Tâche_de_fond_SAPI.CancelAsync()
                    'Réinitialisation des variables

                    Tâche_de_fond_SAPI.WorkerSupportsCancellation = True

                    'on rend de nouveau actif la case à cocher  "CheckBox_Activation_synthese_vocale"
                    Me.CheckBox_Activation_synthese_vocale.Enabled = True

                    ' on sort du sub
                    Exit Sub

                End Try

            End If

            'on passe l'interval du timer à une valeur plus élevée
            Me.modificationCommandesSapi5TempsDuTimer(2000, My.Application.Info.DirectoryPath)


            'on rend de nouveau actif la case à cocher  "CheckBox_Activation_synthese_vocale"
            Me.CheckBox_Activation_synthese_vocale.Enabled = True
        End If



        If (Me.CheckBox_Activation_synthese_vocale.Checked = False) Then


            Try
                activation_Synthese_vocale(False)


                My.Settings.Activation_Synthese_Vocale = False

                'ON arrête le fonctionnement de SAPI5
                'on indique qu'il est en fonctionnement
                Me.modification_commandes_SAPI5_ETAT_DE_FONCTIONNEMENT_SAPI(False, My.Application.Info.DirectoryPath)
                Me.SAPI5_en_cours_execution = False

            Catch ex As Exception
                My.Settings.Activation_Synthese_Vocale = True
                activation_Synthese_vocale(True)
                Me.CheckBox_Activation_synthese_vocale.Checked = True
            End Try



        End If


        'on rend de nouveau actif la case à cocher  "CheckBox_Activation_synthese_vocale"
        Me.CheckBox_Activation_synthese_vocale.Enabled = True

    End Sub


