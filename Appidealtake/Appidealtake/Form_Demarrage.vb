    Function CréationFichierXml(ByVal chemin_du_répertoire_des_données_locales, ByVal Nom_du_répertoire, ByVal MySql_Obj, ByVal Nom_de_la_table, ByVal langue_logiciel)
        Dim Résultat_Création_fichier_xml As String = ""

        Try


            Dim TABLE_MYSQL_COMMAND = MySql_Obj.CreateCommand
            'SELECT * FROM TEST03_SERVER.VERSIONS_TABLES


            If (Nom_de_la_table = "VERSIONS_TABLES") Then
                TABLE_MYSQL_COMMAND.CommandText = "SELECT * " & _
                    " FROM " & TABLE_MYSQL_COMMAND.Connection.Database & "." & Nom_de_la_table & " ORDER BY `ID` DESC LIMIT 1;"
            Else

                TABLE_MYSQL_COMMAND.CommandText = "SELECT * " & _
                    " FROM " & TABLE_MYSQL_COMMAND.Connection.Database & "." & Nom_de_la_table & ";"
            End If
            TABLE_MYSQL_COMMAND.CommandTimeout = 15


            Dim TABLE_MYSQL_DataAdapter As New MySql.Data.MySqlClient.MySqlDataAdapter
            Dim TABLE_DataSet As New DataSet
            Dim TABLE_DataTable As New Data.DataTable

            TABLE_MYSQL_DataAdapter.SelectCommand = TABLE_MYSQL_COMMAND
            TABLE_MYSQL_DataAdapter.Fill(TABLE_DataSet, Nom_de_la_table)


            TABLE_DataTable = TABLE_DataSet.Tables(Nom_de_la_table)

            Dim chemin As String = My.Application.Info.DirectoryPath & "\" & chemin_du_répertoire_des_données_locales & "\" & Nom_du_répertoire & "\" & Nom_de_la_table & ".xml"

            If (FileIO.FileSystem.FileExists(chemin) = True) Then
                'On supprime le fichier avant de le réécrire
                FileIO.FileSystem.DeleteFile(chemin)
            End If

            TABLE_DataTable.WriteXml(chemin)



            Select Case langue_logiciel
                Case 0
                    Résultat_Création_fichier_xml = vbCrLf & "Schaffung der Datei : " & Nom_de_la_table & ".xml"
                Case 1
                    Résultat_Création_fichier_xml = vbCrLf & "Creation of the file : " & Nom_de_la_table & ".xml"
                Case 2
                    Résultat_Création_fichier_xml = vbCrLf & "Creación del fichero : " & Nom_de_la_table & ".xml"
                Case 3
                    Résultat_Création_fichier_xml = vbCrLf & "Création du fichier : " & Nom_de_la_table & ".xml"
                Case 4
                    Résultat_Création_fichier_xml = vbCrLf & "Creazione dello schedario : " & Nom_de_la_table & ".xml"
            End Select

        Catch ex As Exception


            Select Case langue_logiciel
                Case 0
                    Résultat_Création_fichier_xml = vbCrLf & "Ein Irrtum hat sich ereignet : " & ex.Message & ", " & Nom_de_la_table & ".xml"
                Case 1
                    Résultat_Création_fichier_xml = vbCrLf & "An error occurred : " & ex.Message & ", " & Nom_de_la_table & ".xml"
                Case 2
                    Résultat_Création_fichier_xml = vbCrLf & "Un error se produjo : " & ex.Message & ", " & Nom_de_la_table & ".xml"
                Case 3
                    Résultat_Création_fichier_xml = vbCrLf & "Une erreur s'est produite : " & ex.Message & ", " & Nom_de_la_table & ".xml"
                Case 4
                    Résultat_Création_fichier_xml = vbCrLf & "Un errore si è prodursi : " & ex.Message & ", " & Nom_de_la_table & ".xml"
            End Select

        End Try

        Return Résultat_Création_fichier_xml

    End Function

