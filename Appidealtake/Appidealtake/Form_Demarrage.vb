    Private Function IsMailValide(ByVal mailAddress$) As Boolean
        Dim pattern$ = _
        "^([a-zA-Z0-9_\-\.]+)@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)" & _
        "|(([a-zA-Z0-9\-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$"
        Return Regex.IsMatch(mailAddress, pattern, RegexOptions.IgnoreCase)
    End Function

    Private Function IsStringBeginWithNumber(ByVal TestString$) As Boolean
        Dim pattern$ = _
        "[0-9]"
        Return Regex.IsMatch(TestString$, pattern, RegexOptions.None)
    End Function


