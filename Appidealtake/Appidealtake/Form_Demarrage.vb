    Private Function ConstructionPageHtmlDetailsEnregistrement(ByVal ResultatsRecherche As RetourRechercheDetails, _
                                                   ByVal StrSqueletteHTML As String, _
                                                   ByVal langueUtilisée As Integer, _
                                                   ByVal CouleurDeFondString As String, _
                                                   ByVal WebbrowserDestination As Integer)

        'motifs utilisée dans le squelette
        '[INFORMATIONS_RECHERCHE]
        '[COULEUR_FOND]
        '[AUTEUR]
        '[ENREGISTREMENT]
        '[Titre]
        '[Sous_Titre]
        '[Commentaire]
        '[DATE_DE_CREATION]
        '[DATE_DE_MODIFICATION]
        '[REFERENCES]
        '[LIGNE1]
        '[/LIGNE1]
        '[DOCUMENT]
        '[URL_Nom_Document]
        '[Nom_Document]
        '[Type_Document]
        '[/DOCUMENT]
        '[ID]
        '[WEBID]

        Dim CodePageHTML As String = ""

        CodePageHTML = StrSqueletteHTML

        If CodePageHTML.Length = 0 Then Return ""

        CodePageHTML = CodePageHTML.Replace("\r\n", vbCrLf)

        '[INFORMATIONS_RECHERCHE]
        Dim InfosRecherche As String = ResultatsRecherche.InfosRequêtes
        Dim x01 As Integer = 0

        InfosRecherche = InfosRecherche.Replace(vbCrLf, " ")

        For x01 = 1 To InfosRecherche.Length Step 1
            If (x01 * 100) > InfosRecherche.Length Then Exit For
            InfosRecherche = InfosRecherche.Insert(x01 * 100, "\" & vbCrLf)
        Next

        'InfosRecherche = InfosRecherche.Replace("`", "\`")
        InfosRecherche = InfosRecherche.Replace("`", " ")
        'InfosRecherche = InfosRecherche & "\"
        InfosRecherche = InfosRecherche.Replace("\\", "\")
        InfosRecherche = InfosRecherche.Replace("""", " ")
        InfosRecherche = InfosRecherche.Replace("'", " ")

        CodePageHTML = CodePageHTML.Replace("[INFORMATIONS_RECHERCHE]", InfosRecherche)


        '[COULEUR_FOND]
        CodePageHTML = CodePageHTML.Replace("[COULEUR_FOND]", CouleurDeFondString)

        '[AUTEUR]
        CodePageHTML = CodePageHTML.Replace("[AUTEUR]", ResultatsRecherche.RésultatsDeRecherche.Item(0).Auteur)

        '[ENREGISTREMENT]
        CodePageHTML = CodePageHTML.Replace("[ENREGISTREMENT]", ResultatsRecherche.RésultatsDeRecherche.Item(0).Numéro_enregistrement)

        '[Titre]
        CodePageHTML = CodePageHTML.Replace("[Titre]", ResultatsRecherche.RésultatsDeRecherche.Item(0).Titre)

        '[Sous_Titre]
        CodePageHTML = CodePageHTML.Replace("[Sous_Titre]", ResultatsRecherche.RésultatsDeRecherche.Item(0).Sous_Titre)

        '[Commentaire]
        CodePageHTML = CodePageHTML.Replace("[Commentaire]", ResultatsRecherche.RésultatsDeRecherche.Item(0).Commentaire)

        '[DATE_DE_CREATION]
        CodePageHTML = CodePageHTML.Replace("[DATE_DE_CREATION]", ResultatsRecherche.RésultatsDeRecherche.Item(0).DateDeCreation)

        '[DATE_DE_MODIFICATION]
        CodePageHTML = CodePageHTML.Replace("[DATE_DE_MODIFICATION]", ResultatsRecherche.RésultatsDeRecherche.Item(0).DateDeModification)

        '[DESTINATION_RETOUR]
        'WebbrowserDestination = 1  ouvrir le fichier rs.html dans webbrowser1
        'WebbrowserDestination = 2  ouvrir le fichier ra.html dans webbrowser2
        'WebbrowserDestination = 3  ouvrir le fichier rg.html dans webbrowser3
        If (WebbrowserDestination = 1) Then

            If (File.Exists(My.Application.Info.DirectoryPath & "\rs.html") = True) Then
                ' WebBrowser1.Url = New System.Uri(My.Application.Info.DirectoryPath & "\rs.html") 'Lit le fichier de résultat de Recherche
                CodePageHTML = CodePageHTML.Replace("[DESTINATION_RETOUR]", My.Application.Info.DirectoryPath & "\rs.html")
                CodePageHTML = CodePageHTML.Replace("[WEBID]", "WebBrowser1")
            End If

        End If

        If (WebbrowserDestination = 2) Then

            If (File.Exists(My.Application.Info.DirectoryPath & "\ra.html") = True) Then
                CodePageHTML = CodePageHTML.Replace("[DESTINATION_RETOUR]", My.Application.Info.DirectoryPath & "\ra.html")
                CodePageHTML = CodePageHTML.Replace("[WEBID]", "WebBrowser2")
            End If

        End If

        If (WebbrowserDestination = 3) Then
            If (File.Exists(My.Application.Info.DirectoryPath & "\rg.html") = True) Then
                CodePageHTML = CodePageHTML.Replace("[DESTINATION_RETOUR]", My.Application.Info.DirectoryPath & "\rg.html")
                CodePageHTML = CodePageHTML.Replace("[WEBID]", "WebBrowser3")
            End If
        End If



        '[REFERENCES]
        Dim HTMLDesRéférences As String = ""
        Dim x02 As Integer = 0

        For x01 = 0 To (ResultatsRecherche.RésultatsDeRecherche.Item(0).ListeDeRéférence.Count - 1) Step 1

            HTMLDesRéférences += "<a "
            If (ResultatsRecherche.RésultatsDeRecherche.Item(0).ListeDeRéférence.Item(x01).photo.Length > 3) Then
                HTMLDesRéférences += "href =""" & ResultatsRecherche.RésultatsDeRecherche.Item(0).ListeDeRéférence.Item(x01).photo & """  target = ""new" & x01.ToString & """"
            Else
                HTMLDesRéférences += "href =""javascript:;"" "
            End If

            HTMLDesRéférences += " title =""" & ResultatsRecherche.RésultatsDeRecherche.Item(0).ListeDeRéférence.Item(x01).Constructeur & " - " & _
                                  ResultatsRecherche.RésultatsDeRecherche.Item(0).ListeDeRéférence.Item(x01).Catégorie & " - " & _
                                  ResultatsRecherche.RésultatsDeRecherche.Item(0).ListeDeRéférence.Item(x01).Description & """ " & _
                                  ">" & ResultatsRecherche.RésultatsDeRecherche.Item(0).ListeDeRéférence.Item(x01).Référence & "</a>"

            x02 += 1
            If x02 = 4 Then   'Nombre de références par ligne = 4
                HTMLDesRéférences += "<br />"
                x02 = 0
            Else
                If (x01 <> ResultatsRecherche.RésultatsDeRecherche.Item(0).ListeDeRéférence.Count - 1) Then
                    HTMLDesRéférences += "&nbsp;|&nbsp;"
                End If

            End If
        Next
        CodePageHTML = CodePageHTML.Replace("[REFERENCES]", HTMLDesRéférences)


        Dim PartieHaute As String = ""
        PartieHaute = CodePageHTML.Substring(0, CodePageHTML.IndexOf("[LIGNE1]"))

        Dim PartieBasse As String = ""
        PartieBasse = CodePageHTML.Substring(CodePageHTML.IndexOf("[/LIGNE1]") + "[/LIGNE1]".Length)

        Dim PartieDocument As String = ""

        Dim DébutLigne As String = ""
        Dim FinLigne As String = ""
        Dim HTMLDocuments As String = ""

        Dim NmbreDocumentsParLigne As Integer = 0

        If (ResultatsRecherche.RésultatsDeRecherche.Item(0).ListeDocuments.Count > 0) Then

            PartieDocument = CodePageHTML.Substring(CodePageHTML.IndexOf("[LIGNE1]") + "[LIGNE1]".Length)
            PartieDocument = PartieDocument.Substring(0, PartieDocument.IndexOf("[/LIGNE1]"))

            DébutLigne = PartieDocument.Substring(0, PartieDocument.IndexOf("[DOCUMENT]"))
            FinLigne = PartieDocument.Substring(PartieDocument.IndexOf("[/DOCUMENT]") + "[/DOCUMENT]".Length)

            PartieDocument = PartieDocument.Substring(PartieDocument.IndexOf("[DOCUMENT]") + "[DOCUMENT]".Length)
            PartieDocument = PartieDocument.Substring(0, PartieDocument.IndexOf("[/DOCUMENT]"))
            'PartieDocument code HTML modèle

            Dim x04 As Integer = 0
            Dim TempDoc As String = ""

            For x01 = 0 To (ResultatsRecherche.RésultatsDeRecherche.Item(0).ListeDocuments.Count - 1) Step 1

                If (x04 = 0) Then HTMLDocuments += DébutLigne

                TempDoc = PartieDocument

                '[URL_Nom_Document]
                TempDoc = TempDoc.Replace("[URL_Nom_Document]", ResultatsRecherche.RésultatsDeRecherche.Item(0).ListeDocuments.Item(x01).Lien)

                '[Nom_Document]
                TempDoc = TempDoc.Replace("[Nom_Document]", ResultatsRecherche.RésultatsDeRecherche.Item(0).ListeDocuments.Item(x01).Nom)

                '[Type_Document]
                TempDoc = TempDoc.Replace("[Type_Document]", ResultatsRecherche.RésultatsDeRecherche.Item(0).ListeDocuments.Item(x01).Type)

                '[Taille_Document]
                TempDoc = TempDoc.Replace("[Taille_Document]", ResultatsRecherche.RésultatsDeRecherche.Item(0).ListeDocuments.Item(x01).Taille)
                HTMLDocuments += TempDoc

                '[ID] 
                TempDoc = TempDoc.Replace("[ID]", x01.ToString & "_" & x04.ToString)

                TempDoc = ""
                x04 += 1
                If (x04 = 4) Then x04 = 0 : HTMLDocuments += FinLigne


            Next


        End If


        CodePageHTML = PartieHaute & HTMLDocuments & PartieBasse

Pas_de_résultat:
        Return CodePageHTML
    End Function




