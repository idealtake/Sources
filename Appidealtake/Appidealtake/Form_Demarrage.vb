    Function Indice_DE_TABLE_MAJ_incrémenté_DANS_VERSIONS_TABLES(ByVal connexion As String, ByVal Nom_de_la_table As String)

        Try

            'PHASE 1
            'OBTENIR L'ID MAX.

            'e.g = > "SELECT  MAX(ID) FROM `" & ConnexionClient.Database & "`.`VERSIONS_TABLES`;"

            Dim ID_MAX As Integer = 0
            Dim MySql_Connection_obj As New MySql.Data.MySqlClient.MySqlConnection(connexion)
            Dim MySql_Command_obj As New MySql.Data.MySqlClient.MySqlCommand("SELECT  MAX(ID) FROM `" & MySql_Connection_obj.Database & "`.`VERSIONS_TABLES`;", MySql_Connection_obj)

            Dim MySql_Reader_obj As MySql.Data.MySqlClient.MySqlDataReader

            MySql_Connection_obj.Open()
            MySql_Reader_obj = MySql_Command_obj.ExecuteReader()
            MySql_Reader_obj.Read()
            ID_MAX = MySql_Reader_obj.GetInt32(0)
            MySql_Connection_obj.Close()

            'MsgBox(ID_MAX)



            'PHASE 2
            'M.A.J de l'indice de la table.

            'e.g = > update `alpha`.`VERSIONS_TABLES` VERSIONS_TABLES.elements_winform = VERSIONS_TABLES.elements_winform + 1
            ' where VERSIONS_TABLES.ID=2;


            Dim MySqlCommandText As String = "UPDATE `" & MySql_Connection_obj.Database & "`.`VERSIONS_TABLES` " & _
                                            "SET VERSIONS_TABLES.elements_winform = VERSIONS_TABLES.elements_winform + 1 " & _
                                            "WHERE VERSIONS_TABLES.ID=" & ID_MAX & ";"


            Dim MySql_Connection_obj2 As New MySql.Data.MySqlClient.MySqlConnection(connexion)
            Dim MySql_Command_obj2 As New MySql.Data.MySqlClient.MySqlCommand(MySqlCommandText, MySql_Connection_obj2)


            Dim MySql_Reader_obj2 As MySql.Data.MySqlClient.MySqlDataReader

            MySql_Connection_obj2.Open()
            MySql_Reader_obj2 = MySql_Command_obj2.ExecuteReader()
            MySql_Reader_obj2.Read()

            MySql_Connection_obj2.Close()

        Catch ex As Exception

        End Try

        Return True

    End Function




