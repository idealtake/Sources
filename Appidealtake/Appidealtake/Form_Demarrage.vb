    Function Reconstruction_Motif_Arbre(ByVal Tree_Path)

        Dim motif As String = ""
        Dim Position As Integer = 0
        Dim Nombre_de_slash As Integer = 0

        'MsgBox(Tree_Path)
        '------------------------------------------------
        ' Détermine le nombre de slash dans le Tree_path
        ' Permettra la définition de la dimension de table()
        For Position = 0 To Tree_Path.ToString.Length - 1

            If (Tree_Path.ToString.Chars(Position) = "/") Then
                Nombre_de_slash += 1
            End If

        Next

        'MsgBox(Tree_Path.Length)

        'If (Nombre_de_slash = 0 And _
        '  Tree_Path.Length > 0) Then

        'on n'a pas compté de slash mais le tree_path n'est pas vide
        'Nombre_de_slash = 1
        'End If

        Dim Table(Nombre_de_slash)

        '------------------------------------------------
        ' On met dans Table() les différentes chaînes
        ' Du Tree_path
        Position = 0
        For Each noeud As String In Tree_Path.ToString.Split("/")
            Table(Position) = noeud
            Position += 1
        Next

        Dim Nombre_elements_dans_Tree_path = 1 + Nombre_de_slash  ' On part de 0 Pour le 1er élément


        '--------------------------------------------------
        ' On fait une recherche direct du motif par rapport
        ' A la dernière table racine_2 par exemple /Portier/Audio/A BP
        ' Si il n'y a qu'une fois l'occurence "A BP" dans la table racine_2
