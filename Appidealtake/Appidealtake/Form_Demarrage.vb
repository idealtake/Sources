        DEBUT_PAGE = Code_Page_HTML.Substring(0, Code_Page_HTML.IndexOf("[LIGNE1]"))

        Dim FIN_PAGE As String = ""

        FIN_PAGE = Code_Page_HTML.Substring(Code_Page_HTML.IndexOf("[/LIGNE2]"))
        FIN_PAGE = FIN_PAGE.Replace("[/LIGNE2]", "")

        Dim position_debut_ligne1 As Integer = 0
