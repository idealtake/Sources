    Function Recherche_INDICE_AUTO_valeur_nulle(ByVal nom_base_de_données As String, ByVal nom_de_la_table As String, ByVal Infos_sur_la_requête_avancée As String, Mysql_connection As MySql.Data.MySqlClient.MySqlConnection)

        Dim return_struct As New RetourRechercheValeurNulle
        Dim Infos_sur_la_requête As String = ""
        Dim liste_INDICE_AUTO As New List(Of String)


        ' Dim INDICE_AUTO_valeur_nulle_TABLE As String = ""

        Dim Requête_INDICE_AUTO_Valeur_nulle_TABLE = "SELECT  DISTINCT INDICE_AUTO  FROM `" & nom_base_de_données & "`.`" & nom_de_la_table & "` WHERE Signification_Valeur = ""Valeur Nulle"";"


        Infos_sur_la_requête += vbCrLf & "Requêtes_SQL_Recherche_Avancée() - enregistrements - INDICE_AUTO valeur_nulle " & nom_de_la_table & vbCrLf
        Infos_sur_la_requête += Requête_INDICE_AUTO_Valeur_nulle_TABLE & vbCrLf

        Try
            Dim myCommand_18022012a As New MySql.Data.MySqlClient.MySqlCommand(Requête_INDICE_AUTO_Valeur_nulle_TABLE, Mysql_connection)
            Dim myReader_18022012a As MySql.Data.MySqlClient.MySqlDataReader

            myReader_18022012a = myCommand_18022012a.ExecuteReader()

            While myReader_18022012a.Read()
                Infos_sur_la_requête += myReader_18022012a.GetString(0) & vbCrLf
                liste_INDICE_AUTO.Add(myReader_18022012a.GetString(0))
            End While

            return_struct.Résultats_de_recherche = liste_INDICE_AUTO

            myReader_18022012a.Close()
            Infos_sur_la_requête += "Requêtes_SQL_Recherche_Avancée() - enregistrements - INDICE_AUTO valeur_nulle " & nom_de_la_table & " OK" & vbCrLf

            return_struct.Erreur = False

        Catch ex As Exception
            return_struct.Erreur = True
            Infos_sur_la_requête += "Requêtes_SQL_Recherche_Avancée() - enregistrements - INDICE_AUTO valeur_nulle " & nom_de_la_table & " BAD" & vbCrLf
            Infos_sur_la_requête += ex.Message & vbCrLf & vbCrLf

        End Try

        return_struct.Infos_requêtes = Infos_sur_la_requête

        Return return_struct

    End Function

