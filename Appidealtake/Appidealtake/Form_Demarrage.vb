    Function Formattage_date_heure(ByVal Langue As Integer)
        Dim Date_et_Heure_formattée As String = ""

        Dim dt As DateTime = DateTime.Now


        'Valeur de Langue
        ' 0 = Allemand
        ' 1 = Anglais
        ' 2 = Espagnole
        ' 3 = Français
        ' 4 = Italien

        Select Case Langue
            Case 0

                ' Creates a CultureInfo for German in Germany.
                Dim ci As New CultureInfo("de-DE")

                ' Displays dt, formatted using the ShortDatePattern
                ' and the CultureInfo.  
                Date_et_Heure_formattée = dt.ToString("f", ci)

            Case 1

                ' Creates a CultureInfo for US. English
                Dim ci As New CultureInfo("en-US")

                ' Displays dt, formatted using the ShortDatePattern
                ' and the CultureInfo.  
                Date_et_Heure_formattée = dt.ToString("f", ci)

            Case 2

                ' Creates a CultureInfo for Spanish - Spain.
                Dim ci As New CultureInfo("es-ES")

                ' Displays dt, formatted using the ShortDatePattern
                ' and the CultureInfo.  
                Date_et_Heure_formattée = dt.ToString("f", ci)

            Case 3
                ' Creates a CultureInfo for French.
                Dim ci As New CultureInfo("fr-FR")

                ' Displays dt, formatted using the ShortDatePattern
                ' and the CultureInfo.  
                Date_et_Heure_formattée = dt.ToString("f", ci)

            Case 4
                ' Creates a CultureInfo for Italian-Italy
                Dim ci As New CultureInfo("it-IT")

                ' Displays dt, formatted using the ShortDatePattern
                ' and the CultureInfo.  
                Date_et_Heure_formattée = dt.ToString("f", ci)
        End Select



        Return Date_et_Heure_formattée
    End Function

