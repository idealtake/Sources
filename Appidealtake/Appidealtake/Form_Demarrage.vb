    Public Sub SupprimeEnregistrement(ByVal StrChrono As String)

        'AuthentificationGestionBase est le nom de l'utilisateur authentifié
        'Si la chaîne est vide alors on affiche la page de gestion des utilisateurs
        If (AuthentificationGestionBase.Length = 0) Then
            AffichageCompte_GestionBase()
            Exit Sub
        End If


        'si la variable ne contient rien c'est un premier chargement
        If ChronoEnregistrementASupprimer = "" Then

            'on sauve le chrono à supprimer dans une variable globale
            ChronoEnregistrementASupprimer = StrChrono
            If (ChronoEnregistrementASupprimer = "") Then Exit Sub

            'Ici on charge la page  "Supprime_enregistrement.html"
            'La page a été appellé par Recherche enregistrement
            'c'est le premier chargement de "Supprime_enregistrement.html"
            AffichageSupprimer_Enregistrement()

        Else
            'la variable est déjà sauvée la page a déjà été chargée
            'on va faire la requête de suppression

            Dim document As System.Windows.Forms.HtmlDocument = WebBrowser_Gestion_Base.Document



            Dim ArgumentSupprimerEnregistrement As New Argument_Supprimer_Enregistrement
            ArgumentSupprimerEnregistrement.Chrono = ChronoEnregistrementASupprimer
            ArgumentSupprimerEnregistrement.Utilisateur = AuthentificationGestionBase
            ArgumentSupprimerEnregistrement.chaine_de_connexion = connexion
            ArgumentSupprimerEnregistrement.langue_logiciel = LangueLogiciel
            ArgumentSupprimerEnregistrement.Nom_de_la_base_de_données = Nom_de_la_base.Text


            'document.All("Etat_Suppression").InnerText = "Vérification en cours ..."
            document.All("Etat_Suppression").InnerText = Ajout_Suppression_connexion_base.MessageUtilisateur(225, LangueLogiciel)

            If Not BackgroundWorkerSupprimeEnregistrement.IsBusy = True Then

                TimerSupprimeEnregistrement.Start()

                BackgroundWorkerSupprimeEnregistrement.RunWorkerAsync(ArgumentSupprimerEnregistrement)




            End If

            'On réinitialise la variable
            ChronoEnregistrementASupprimer = ""

        End If









    End Sub


