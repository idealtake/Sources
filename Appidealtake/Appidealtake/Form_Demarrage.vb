                                Temp1_Nombre_documents = composition_Type_Documents(T1, 3)

                                Temp2_ID_DOC = composition_Type_Documents(T1 + 1, 1)
                                Temp2_Nom_DOC = composition_Type_Documents(T1 + 1, 2)
                                Temp2_Nombre_documents = composition_Type_Documents(T1 + 1, 3)

                                'on inverse les deux enregistrements dans la table
                                composition_Type_Documents(T1, 1) = Temp2_ID_DOC
                                composition_Type_Documents(T1, 2) = Temp2_Nom_DOC
                                composition_Type_Documents(T1, 3) = Temp2_Nombre_documents

                                composition_Type_Documents(T1 + 1, 1) = Temp1_ID_DOC
                                composition_Type_Documents(T1 + 1, 2) = Temp1_Nom_DOC
                                composition_Type_Documents(T1 + 1, 3) = Temp1_Nombre_documents

                            Else
                                trie_OK += 1
                            End If

                        End If

                        If (trie_OK = nombre_TYPE_DOCS) Then
                            Trie_Mauvais = False
                        End If

                        ' on doit contrôler si tout les nombres de documents sont identiques
                        Dim T2 As Integer
                        Dim contrôle As Integer = 0

                        For T2 = 1 To nombre_TYPE_DOCS

                            If (T2 <> nombre_TYPE_DOCS) Then
                                If (composition_Type_Documents(T2, 3) = composition_Type_Documents(T2 + 1, 3)) Then
                                    contrôle += 1
                                End If
                            End If

                        Next

                        If (contrôle = (nombre_TYPE_DOCS - 1)) Then
                            Trie_Mauvais = False
                        End If


                    Next

                End While


                ' On doit fusionner les noms des types doc ainsi qu'aditionner
                ' Leur nombre d'occurence.

