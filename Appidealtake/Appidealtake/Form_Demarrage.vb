    Private Function AlternanceDeRésultatsHTMLRs(ByVal LIGNE1 As String, ByVal LIGNE2 As String, ByVal Resultats_Recherche_Infos As RetourRechercheSimplifiée)

        Dim CODE As String = ""
        Dim ALTERNANCE As Boolean = False
        Dim T As Integer = 0

        If (LIGNE1.Length = 0 And LIGNE2.Length = 0) Then Return ""

        For T = 0 To (Resultats_Recherche_Infos.Résultats_de_recherche.Count - 1)
            Select Case ALTERNANCE
                Case False
                    CODE += LIGNE1
                    ALTERNANCE = True
                Case True
                    CODE += LIGNE2
                    ALTERNANCE = False
            End Select


            '[enregistrement]
            CODE = CODE.Replace("[ENREGISTREMENT]", Resultats_Recherche_Infos.Résultats_de_recherche.Item(T).Numéro_enregistrement)

            '[Titre]
            CODE = CODE.Replace("[Titre]", Resultats_Recherche_Infos.Résultats_de_recherche.Item(T).Titre)

            '[Sous_Titre]
            CODE = CODE.Replace("[Sous_Titre]", Resultats_Recherche_Infos.Résultats_de_recherche.Item(T).Sous_Titre)

            '[Commentaire]

            ' On enléve le retour à la ligne séquence 13 d & 10 d soit $0D $0A par un retour à la ligne <br>
            Dim TempCommentaire As String = Resultats_Recherche_Infos.Résultats_de_recherche.Item(T).Commentaire

            'regarder la chaine TempCommentaire en  hexa
            'Dim bytes As Byte() = System.Text.Encoding.ASCII.GetBytes(TempCommentaire)
            'Dim hex As String = BitConverter.ToString(bytes).Replace("-", String.Empty)
            'en fait vbcrlf est enregistré sous la forme de $0A au lieu de $0D $0A

            'TempCommentaire = TempCommentaire.Replace(Chr(13) & Chr(10), "<br>")
            'TempCommentaire = TempCommentaire.Replace(vbCrLf, "<br>")

            ' On enléve le retour à la ligne séquence 32 d & 10 d soit $20 $0A par un retour à la ligne <br>
            TempCommentaire = TempCommentaire.Replace(Chr(32) & Chr(10), "<br>")

            'on transforme le motif $0A en <br>
            TempCommentaire = TempCommentaire.Replace(Chr(10), "<br>")

            CODE = CODE.Replace("[Commentaire]", TempCommentaire)

            '[CHRONO]
            CODE = CODE.Replace("[CHRONO]", "[" & Resultats_Recherche_Infos.Résultats_de_recherche.Item(T).Chrono & "]")

        Next

        Return CODE
    End Function

