    Private Sub Tâche_de_fond_SAPI_DoWork(ByVal sender As System.Object, ByVal e As System.ComponentModel.DoWorkEventArgs) Handles Tâche_de_fond_SAPI.DoWork
        Dim Travail_SAPI As BackgroundWorker = CType(sender, BackgroundWorker)
        e.Result = Lancement_SAPI(Travail_SAPI, e)

    End Sub

    Function Lancement_SAPI(ByVal worker As BackgroundWorker, ByVal e As DoWorkEventArgs)

        ' start NT tracert command 
        Dim ExeFileName As String
        Dim Arguments As String
        ExeFileName = "SAPI5"
        Arguments = ""
        Dim s As String
        Dim p As New Process



        Dim Chemin_SAPI5 As String = ""

        Chemin_SAPI5 = My.Application.Info.DirectoryPath

        With p.StartInfo
            .WorkingDirectory = Chemin_SAPI5 & "\"
            .FileName = ExeFileName
            .Arguments = Arguments
            .UseShellExecute = False
            .RedirectStandardError = True
            .RedirectStandardInput = True
            .RedirectStandardOutput = True
            .WindowStyle = ProcessWindowStyle.Normal
            .CreateNoWindow = True
        End With

        Try
            p.Start()
            s = p.StandardOutput.ReadToEnd()
            p.WaitForExit()
        Catch ex As Exception
            s = ex.ToString
            p.Dispose()
        End Try
        p.Dispose()



        Return True

    End Function



