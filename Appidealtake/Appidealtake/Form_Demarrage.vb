    Private Sub Timer_Etat_RS_Tick(sender As System.Object, e As System.EventArgs) Handles TimerEtatRS.Tick

        'Type_de_message
        '0 = pas de message
        '1 = Recherche Terminée
        '2 = Erreur
        '3 = BackGroundWorkerRS a construit la page HTML RS 1ère phase
        '4 = BackGroundWorkerRS a construit la page HTML RS 2ème phase
        '5 = En attente

        If My.Settings.TypeMessageRs = 0 Then
            'pas de message
            Exit Sub
        End If

        If My.Settings.TypeMessageRs = 1 Then
            ' Recherche Terminée
            Etat_Avancement(Couleur_fond_résultat_Recherche_simplifiée, WebBrowser1, LangueLogiciel, 1, "")

            My.Settings.TypeMessageRs = 0
            My.Settings.MessageErreurRs = ""
            Exit Sub
        End If

        If My.Settings.TypeMessageRs = 2 Then
            ' Erreur Recherche
            Etat_Avancement(Couleur_fond_résultat_Recherche_simplifiée, WebBrowser1, LangueLogiciel, 2, My.Settings.MessageErreurRs)

            TimerEtatRS.Stop()
            My.Settings.TypeMessageRs = 0
            My.Settings.MessageErreurRs = ""

        End If

        If (My.Settings.TypeMessageRs = 3 And WebBrowser1.IsBusy = False) Then

            ' Le BackGroundWorkerRS a récupéré la 1ere partie des résultats de la Recherche
            'WebBrowser1.Document.OpenNew(True)
            '   ///WebBrowser1.DocumentText = My.Settings.PageHtmlRS
            'WebBrowser1.Document.Write(My.Settings.PageHtmlRS)
            'WebBrowser1.Visible = True
            '   ///WebBrowser1.AllowNavigation = True
            'WebBrowser1.Refresh()


            Try

                Dim flux As FileStream
                Dim ecriture As StreamWriter


                flux = File.Create(My.Application.Info.DirectoryPath & "\rs.html")
                ecriture = New StreamWriter(flux)
                ecriture.Write(My.Settings.PageHtmlRS)

                ecriture.Close()

                WebBrowser1.Url = New System.Uri(My.Application.Info.DirectoryPath & "\rs.html") 'Lit le fichier de résultat de Recherche

                WebBrowser1.Visible = True

                'on rend visible le menu zoom
                MenuStripZoomWeb1.Visible = True

            Catch ex As System.IO.IOException
                Exit Sub
            End Try

            My.Settings.TypeMessageRs = 0
            My.Settings.MessageErreurRs = ""
            My.Settings.PageHtmlRS = ""
            Exit Sub
        End If


        If (My.Settings.TypeMessageRs = 4 And WebBrowser1.IsBusy = False) Then

            ' Le BackGroundWorkerRS a récupéré la 2ème et dernière partie des résultats de la Recherche
            'WebBrowser1.Document.OpenNew(True)
            '   ///WebBrowser1.DocumentText = My.Settings.PageHtmlRS
            'WebBrowser1.Document.Write(My.Settings.PageHtmlRS)
            'WebBrowser1.Visible = True
            '   ///WebBrowser1.AllowNavigation = True
            'WebBrowser1.Refresh()


            Try
                Dim flux As FileStream
                Dim ecriture As StreamWriter


                flux = File.Create(My.Application.Info.DirectoryPath & "\rs.html")
                ecriture = New StreamWriter(flux)
                ecriture.Write(My.Settings.PageHtmlRS)

                ecriture.Close()

                WebBrowser1.Url = New System.Uri(My.Application.Info.DirectoryPath & "\rs.html") 'Lit le fichier de résultat de Recherche

                'on rend visible le menu zoom
                'MenuStrip1.Visible = True

                If WebBrowser1.Visible = False Then
                    ' On a cliqué sur le bouton Annuler
                    ' On ne doit pas rendre visible le WebBrowser
                    ' Le WebBrowser1  n'est pas visible actuellement
                    ' donc MenuStrip1 non plus

                    'on rend invisible le menu zoom
                    MenuStripZoomWeb1.Visible = False

                Else
                    ' ici par contre on a pas cliqué sur le bouton "Annuler" de RS
                    ' on doit rendre visible Menustrip1

                    MenuStripZoomWeb1.Visible = True

                End If


            Catch ex As System.IO.IOException
                Exit Sub
            End Try


            My.Settings.TypeMessageRs = 0
            My.Settings.MessageErreurRs = ""
            My.Settings.PageHtmlRS = ""
            TimerEtatRS.Stop()

        End If

        If (My.Settings.TypeMessageRs = 5 And WebBrowser1.IsBusy = False) Then
            Etat_Avancement(Couleur_fond_résultat_Recherche_simplifiée, WebBrowser1, LangueLogiciel, 0, "")
            My.Settings.TypeMessageRs = 0
        End If

    End Sub



