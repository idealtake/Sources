    Private Function RequêteSQLAuthentificationtUtilisateurGestionBase(ByVal UserName As String, ByVal UserPassword As String, ByVal chaine_de_connexion As String, ByVal Nom_de_la_base_de_données As String, ByVal langue_logiciel As Integer)


        Dim Retour As New RetourAjoutCompteGestionBase

        Dim Infos_sur_la_requête_AjoutCompteGestionBase As String = ""


        Infos_sur_la_requête_AjoutCompteGestionBase += "Connexion : " & chaine_de_connexion & vbCrLf

        Try
            ' SELECT count(*) FROM TEST03_SERVER.Users WHERE   User_Name = 'TVIEIL';




            Dim Mysql_connection As New MySql.Data.MySqlClient.MySqlConnection(chaine_de_connexion)
            Mysql_connection.Open()

            Dim Requète_UtilisateurExistant As String = ""
            Requète_UtilisateurExistant = "SELECT COUNT(*) FROM " & Nom_de_la_base_de_données & ".Users WHERE  User_Name = '" & UserName & "' AND Password = '" & UserPassword & "' AND Enable = 1;"
            Infos_sur_la_requête_AjoutCompteGestionBase += vbCrLf & " RequêteSQLAuthentificationtUtilisateurGestionBase()" & vbCrLf
            Infos_sur_la_requête_AjoutCompteGestionBase += Requète_UtilisateurExistant & vbCrLf

            Dim myCommand As New MySql.Data.MySqlClient.MySqlCommand(Requète_UtilisateurExistant, Mysql_connection)
            Dim myReader As MySql.Data.MySqlClient.MySqlDataReader
            myReader = myCommand.ExecuteReader()

            myReader.Read()
            Dim UserTest As String = myReader.GetString(0)
            myReader.Close()

            If (UserTest = "1") Then
                'MsgBox("Compte authentifié.")

                'Si on réussit à enregistrer le nouveau utilisateur on retourne son Nom
                Retour.Nom_Utilisateur_Enregistré = UserName

                Infos_sur_la_requête_AjoutCompteGestionBase += " -> OK"
            Else
                Retour.Nom_Utilisateur_Enregistré = ""
                Infos_sur_la_requête_AjoutCompteGestionBase += " -> BAD"
            End If

            myCommand.Dispose()



fin:        Mysql_connection.Close()
            Infos_sur_la_requête_AjoutCompteGestionBase += vbCrLf & "Déconnexion." & vbCrLf

        Catch ex As MySql.Data.MySqlClient.MySqlException
            'MessageBox.Show(ex.ErrorCode & " : " & ex.Message, "Requêtes_SQL_Recherche_simplifiée() - Error - ")
            Infos_sur_la_requête_AjoutCompteGestionBase += " : " & ex.Message & " RequêteSQLAuthentificationtUtilisateurGestionBase() - Error - " & vbCrLf

            'test si la connection mysql est toujours ouverte
            'If Mysql_connection.State = ConnectionState.Open Then
            'si oui on la ferme
            'Mysql_connection.Close()
            'End If

            GoTo erreur_fin
        End Try
        Retour.Erreur = False
        Retour.Infos_requêtes = Infos_sur_la_requête_AjoutCompteGestionBase

        Return Retour

erreur_fin:
        Retour.Erreur = True
        Retour.Infos_requêtes = Infos_sur_la_requête_AjoutCompteGestionBase
        Return Retour

    End Function

    Public Sub AuthentificationUtilisateurGestionBase()
        'MsgBox("Authentification d'un Utilisateur")


        Dim document As System.Windows.Forms.HtmlDocument = WebBrowser_Gestion_Base.Document


        Dim ArgumentCréationUtilisateur As New Argument_UtilisateurGestionBase
        ArgumentCréationUtilisateur.UserName = document.All("User_Name_Authentication").GetAttribute("value").ToString
        ArgumentCréationUtilisateur.UserName = LTrim(ArgumentCréationUtilisateur.UserName)
        ArgumentCréationUtilisateur.UserName = RTrim(ArgumentCréationUtilisateur.UserName)


        ArgumentCréationUtilisateur.UserPassword = document.All("Mot_de_passe_Password_Authentication").GetAttribute("value").ToString
        ArgumentCréationUtilisateur.UserPassword = LTrim(ArgumentCréationUtilisateur.UserPassword)
        ArgumentCréationUtilisateur.UserPassword = RTrim(ArgumentCréationUtilisateur.UserPassword)


        If (String.IsNullOrWhiteSpace(ArgumentCréationUtilisateur.UserName) = True And _
            String.IsNullOrWhiteSpace(ArgumentCréationUtilisateur.UserPassword) = True) Then

            'Pas de nom d'utilisateur ni mot de passe
            'MsgBox("Pas de vérification à réaliser")

            Exit Sub
        End If


        If (String.IsNullOrWhiteSpace(ArgumentCréationUtilisateur.UserName) = True Or ArgumentCréationUtilisateur.UserName.Length < 6) Then
            'If (ArgumentCréationUtilisateur.UserName.Length < 6) Then

            'Veuillez saisir votre nom d'utilisateur 6 caractères minimum.
            MsgBox(Ajout_Suppression_connexion_base.MessageUtilisateur(210, LangueLogiciel), MsgBoxStyle.Information, Ajout_Suppression_connexion_base.MessageUtilisateur(22, LangueLogiciel))

            Exit Sub
        End If


        If (ArgumentCréationUtilisateur.UserName.Length > 45) Then

            'Votre nom d'utilisateur est trop long 45 caractères Maximum.
            MsgBox(Ajout_Suppression_connexion_base.MessageUtilisateur(220, LangueLogiciel), MsgBoxStyle.Information, Ajout_Suppression_connexion_base.MessageUtilisateur(22, LangueLogiciel))

            Exit Sub
        End If

        If (String.IsNullOrWhiteSpace(ArgumentCréationUtilisateur.UserPassword) = True Or ArgumentCréationUtilisateur.UserPassword.Length < 6) Then

            'Veuillez saisir votre mot de passe 6 caractères minimum.
            MsgBox(Ajout_Suppression_connexion_base.MessageUtilisateur(211, LangueLogiciel), MsgBoxStyle.Information, Ajout_Suppression_connexion_base.MessageUtilisateur(22, LangueLogiciel))

            Exit Sub
        End If


        If (ArgumentCréationUtilisateur.UserPassword.Length > 45) Then

            'Votre mot de passe est trop long 45 caractères Maximum.
            MsgBox(Ajout_Suppression_connexion_base.MessageUtilisateur(217, LangueLogiciel), MsgBoxStyle.Information, Ajout_Suppression_connexion_base.MessageUtilisateur(22, LangueLogiciel))

            Exit Sub
        End If

        'On va crypter le mot de passe et on le fait passer en BASE64

        ArgumentCréationUtilisateur.chaine_de_connexion = connexion
        ArgumentCréationUtilisateur.Nom_de_la_base_de_données = Nom_de_la_base.Text
        ArgumentCréationUtilisateur.langue_logiciel = LangueLogiciel

        Const MyKey As String = "ptdmaupt5_dfzpu"
        Const MyIV As String = "9fo3qPj96TbA90l"

        Dim Crypto1 = New CryptoUtil(MyKey, MyIV)

        'ArgumentCréationUtilisateur.UserPassword 
        ArgumentCréationUtilisateur.UserPassword = Crypto1.EncryptText(ArgumentCréationUtilisateur.UserPassword)
        'Puis on convertit en base 64
        ArgumentCréationUtilisateur.UserPassword = ConvertStringToBase64(ArgumentCréationUtilisateur.UserPassword)

        'document.All("Etat_Recherche").InnerText = "Vérification en cours..."
        document.All("Etat_Identification").InnerText = Ajout_Suppression_connexion_base.MessageUtilisateur(225, LangueLogiciel)

