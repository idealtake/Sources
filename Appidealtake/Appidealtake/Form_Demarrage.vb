    Function Utilisateur_Mot_de_passe_droit()
        'infos sur le tableau contenant les informations Utilisateurs
        ' Compte_Utilisateurs(100, 3) As String 
        ' Nom d'utilisateur, Mot de passe, Droit Gestionnaire ou Administrateur
        Const MyKey As String = "CM3125nlp-6&pmo"
        Const MyIV As String = "Contest_Decrypt"

        Crypto = New CryptoUtil(MyKey, MyIV)

        If My.Computer.FileSystem.FileExists("Users_Password_Right.txt") Then
            'le fichier crypté existe déjà on récupère tout son contenue on le décrypte
            'on stocke dans le tableau Compte_Utilisateurs(100, 3)
            Try
                ConnStr = Crypto.DecryptageTexteduFichier("Users_Password_Right.txt")
                'MsgBox(ConnStr)
                '.Split("|")


                Dim ligne_du_fichier_en_entree As String()
                ligne_du_fichier_en_entree = ConnStr.Split("_")

                Dim s As String
                Dim counter_1 As Integer = 0

                For Each s In ligne_du_fichier_en_entree

                    If (Microsoft.VisualBasic.Strings.Len(s) > 2) Then

                        ligne_du_fichier_en_entree(counter_1).Replace(vbNewLine, "") 'supprime d'éventuel retour de ligne

                        Dim position_1 As String
                        Dim part1 As String 'nom d'utilisateur 

                        'MsgBox(ligne_du_fichier_en_entree(counter_1))

                        position_1 = ligne_du_fichier_en_entree(counter_1).IndexOf("|")        'récupère la position de la 1ere |
                        part1 = ligne_du_fichier_en_entree(counter_1).Substring(0, position_1) ' récupère le nom du 1er utilisateur
                        'MsgBox(part1)
                        ligne_du_fichier_en_entree(counter_1) = ligne_du_fichier_en_entree(counter_1).Substring((position_1 + 1)) 'restocke sans le nom utilisateur
                        'MsgBox(ligne_du_fichier_en_entree(counter_1))

                        Dim position_2 As String
                        Dim part2 As String  'code de l'utilisateur 
                        position_2 = ligne_du_fichier_en_entree(counter_1).IndexOf("|")        'récupère la position de la 1ere |
                        part2 = ligne_du_fichier_en_entree(counter_1).Substring(0, position_2) ' récupère le code de l' utilisateur
                        ligne_du_fichier_en_entree(counter_1) = ligne_du_fichier_en_entree(counter_1).Substring((position_2 + 1)) 'restocke sans le code utilisateur

                        Dim part3 As String 'droit d'utilisateur 
                        part3 = ligne_du_fichier_en_entree(counter_1)                          'récupère le droit utilisateur

                        part1 = LTrim(part1)
                        part1 = RTrim(part1)

                        part2 = LTrim(part2)
                        part2 = RTrim(part2)

                        part3 = LTrim(part3)
                        part3 = RTrim(part3)

                        Compte_Utilisateurs(counter_1, 1) = part1
                        Compte_Utilisateurs(counter_1, 2) = part2
                        Compte_Utilisateurs(counter_1, 3) = part3

                        'MsgBox(Compte_Utilisateurs(counter_1, 1) & "," & Compte_Utilisateurs(counter_1, 2) & "," & Compte_Utilisateurs(counter_1, 3))

                        counter_1 += 1 'counter_1
                        ' MsgBox(counter_1)
                    End If
                Next s
                'MsgBox(Compte_Utilisateurs(1, 1) & "," & Compte_Utilisateurs(1, 2) & "," & Compte_Utilisateurs(1, 3))

            Catch ex As System.Exception

                Dim infosLog As String = ""
                infosLog = "Function EncryptageTexteduFichier() :Not OK " & vbCrLf & ex.Message
                Me.Log_Idealtake(My.Application.Info.DirectoryPath, infosLog)

                MessageBox.Show(ex.Message, "Erreur_1 Utilisateur_Mot_de_passe_droit()", MessageBoxButtons.OK, MessageBoxIcon.Error)
            End Try
        Else

            'Charge en mémoire le premier compte d'administrateur
            Compte_Utilisateurs(1, 1) = "Administrateur"
            Compte_Utilisateurs(1, 2) = "PA2X2LA"
            Compte_Utilisateurs(1, 3) = "Administrateur"

            'Charge en mémoire le deuxième compte de gestionnaire
            Compte_Utilisateurs(2, 1) = "Gestionnaire"
            Compte_Utilisateurs(2, 2) = "485156"
            Compte_Utilisateurs(2, 3) = "Gestionnaire"

            'Charge en mémoire un troisième compte de TEST
            'Vérification de la prise en charge
            'de l 'erreur de droit d'utilisateur non valide 16/06/2007
            ' OK ça Marrche
            'Compte_Utilisateurs(2, 1) = "TEST"
            'Compte_Utilisateurs(2, 2) = "485156"
            'Compte_Utilisateurs(2, 3) = "Ministre"

            ' et les sauvegarder de façon cryptés
            Dim Str_comptes_utilisateurs As String = ""

            'Comptes par défaut
            Str_comptes_utilisateurs = "Administrateur|PA2X2LA|Administrateur" & "_" & "Gestionnaire|485156|Gestionnaire" & "_"

            'rajoute un compte de test
            ' Vérification de la prise en charge de l'erreur de droit d'utilisateur non valide 16/06/2007
            'Str_comptes_utilisateurs = Str_comptes_utilisateurs & "TEST|485156|Ministre" & "_"

            Try
                'on les enregistres dans un fichier crytpé grace à la classe CryptoUtil
                Crypto.EncryptageTexteduFichier(Str_comptes_utilisateurs, "Users_Password_Right.txt")

            Catch ex As Exception
                MessageBox.Show(ex.Message, "Erreur_2 Utilisateur_Mot_de_passe_droit()", MessageBoxButtons.OK, MessageBoxIcon.Error)
            End Try

        End If


        Return True
    End Function

    Function Nbre_Utilisateur_par_droit(ByVal droit As String)
        'Nombre d'utilisateurs en fonction de leur droit
        Dim T1 As Integer
        Dim counter_utilisateur As Integer = 0

        For T1 = 0 To 100
            If ((Len(Compte_Utilisateurs(T1, 1)) > 2) And (Compte_Utilisateurs(T1, 3) = droit)) Then
                counter_utilisateur += 1
            End If
        Next

        Return counter_utilisateur
    End Function

    Function Nbre_Utilisateur_Gestion_Base()
        'Nombre d'utilisateurs actuellement
        Dim T1 As Integer
        Dim counter_utilisateur As Integer = 0

        For T1 = 0 To 100
            If (Len(Compte_Utilisateurs(T1, 1)) > 2) Then
                counter_utilisateur += 1
            End If
        Next

        Return counter_utilisateur
    End Function

    Function Premiere_case_memoire_libre_tableau_Utilisateur_Gestion_Base()
        'Nombre d'utilisateurs actuellement
        Dim T1 As Integer
        Dim counter_utilisateur As Integer = 0

        For T1 = 0 To 100
            If (Len(Compte_Utilisateurs(T1, 1)) = 0) Then
                Exit For
            End If
        Next

        Return T1
    End Function

    Function Position_Utilisateur_dans_le_tableau(ByVal Nom_Utilisateur As String)


        'position dans le tableau d'utilisateur d'un Nom d'utilisateur

        Dim T1 As Integer
        Dim position_utilisateur As Integer = 0

        For T1 = 0 To 100

            If (Compte_Utilisateurs(T1, 1) = Nom_Utilisateur) Then
                Exit For
            End If

        Next

        position_utilisateur = T1

        Return position_utilisateur

    End Function

