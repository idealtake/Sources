    Private Function RequêteSQLAutorisationSupprimerEnregistrement(ByVal chrono As String, ByVal Utilisateur As String, ByVal ChaineDeConnexion As String, ByVal NomDeLaBaseDeDonnées As String, ByVal LangueFonction As Integer)



        Dim Retour As New RetourVérifiactionUtilisateurAutorisé

        Dim Infos_sur_la_requête_AutorisationSupprimerEnregistrement As String = ""


        Infos_sur_la_requête_AutorisationSupprimerEnregistrement += "Connexion : " & ChaineDeConnexion & vbCrLf

        Try
            ' SELECT count(*) FROM TEST03_SERVER.Users WHERE   User_Name = 'TVIEIL';

            Dim Mysql_connection As New MySql.Data.MySqlClient.MySqlConnection(ChaineDeConnexion)
            Mysql_connection.Open()

            Dim Requète_UtilisateurExistant As String = ""
            Requète_UtilisateurExistant = "SELECT COUNT(Auteur) FROM " & NomDeLaBaseDeDonnées & ".enregistrements WHERE  Auteur = '" & Utilisateur & "'  AND Chrono = " & chrono & " ;"
            Infos_sur_la_requête_AutorisationSupprimerEnregistrement += vbCrLf & " RequêteSQLAutorisationSupprimerEnregistrement()" & vbCrLf
            Infos_sur_la_requête_AutorisationSupprimerEnregistrement += Requète_UtilisateurExistant & vbCrLf

            Dim myCommand As New MySql.Data.MySqlClient.MySqlCommand(Requète_UtilisateurExistant, Mysql_connection)
            Dim myReader As MySql.Data.MySqlClient.MySqlDataReader
            myReader = myCommand.ExecuteReader()


            myReader.Read()
            Dim UserTest As String = myReader.GetString(0)

            myReader.Close()

            If (UserTest = "1") Then
                'MsgBox("Compte retrouvé.")

                'Utilisateur Autorisé à supprimer l'enregistrement on retourne  '1'
                Retour.RetourVérifiaction = 1
                Infos_sur_la_requête_AutorisationSupprimerEnregistrement += " -> OK"

            Else

                'Utilisateur non Autorisé à supprimer l'enregistrement on retourne  '0'
                Retour.RetourVérifiaction = 0
                Infos_sur_la_requête_AutorisationSupprimerEnregistrement += " -> BAD"

            End If

            myCommand.Dispose()



fin:        Mysql_connection.Close()
            Infos_sur_la_requête_AutorisationSupprimerEnregistrement += vbCrLf & "Déconnexion." & vbCrLf

        Catch ex As MySql.Data.MySqlClient.MySqlException
            'MessageBox.Show(ex.ErrorCode & " : " & ex.Message, "Requêtes_SQL_Recherche_simplifiée() - Error - ")
            Infos_sur_la_requête_AutorisationSupprimerEnregistrement += " : " & ex.Message & " RequêteSQLAutorisationSupprimerEnregistrement() - Error - " & vbCrLf

            'test si la connection mysql est toujours ouverte
            'If Mysql_connection.State = ConnectionState.Open Then
            'si oui on la ferme
            'Mysql_connection.Close()
            'End If

            GoTo erreur_fin
        End Try
        Retour.Erreur = False
        Retour.Infos_requêtes = Infos_sur_la_requête_AutorisationSupprimerEnregistrement

        Return Retour

erreur_fin:
        Retour.Erreur = True
        Retour.Infos_requêtes = Infos_sur_la_requête_AutorisationSupprimerEnregistrement
        Return Retour

    End Function

