                                point_suspension_ok = True
                            Else
                                If (composition_Type_Documents(T3, 3) > 0) Then
                                    Temp3_Nom_DOC = Temp3_Nom_DOC & ", " & composition_Type_Documents(T3, 2) & _
                                     " (" & composition_Type_Documents(T3, 3) & ")"

                                End If
                            End If

                        End If


                    Next
                    composition_Type_Documents(4, 2) = Temp3_Nom_DOC
                    composition_Type_Documents(4, 3) = Temp3_Nombre_documents
                End If

