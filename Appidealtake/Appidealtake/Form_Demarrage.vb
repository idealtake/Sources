    Private Sub Timer_Etat_RG_Tick(sender As System.Object, e As System.EventArgs) Handles TimerEtatRG.Tick
        'Type_de_message
        '0 = pas de message
        '1 = Recherche Terminée
        '2 = Erreur
        '3 = BackGroundWorkerRG a construit la page HTML RS 1ère phase
        '4 = BackGroundWorkerRG a construit la page HTML RS 2ème phase
        '5 = En attente

        If My.Settings.TypeMessageRg = 0 Then
            'pas de message
            Exit Sub
        End If

        If My.Settings.TypeMessageRg = 1 Then
            ' Recherche Terminée
            Etat_Avancement(Couleur_fond_résultat_Recherche_Guidée, WebBrowser3, LangueLogiciel, 1, "")

            'TimerEtatRG.Stop()
            My.Settings.TypeMessageRg = 0
            My.Settings.MessageErreurRg = ""
            Exit Sub
        End If

        If My.Settings.TypeMessageRg = 2 Then
            ' Erreur Recherche
            Etat_Avancement(Couleur_fond_résultat_Recherche_Guidée, WebBrowser3, LangueLogiciel, 2, My.Settings.MessageErreurRg)

            TimerEtatRG.Stop()
            My.Settings.TypeMessageRg = 0
            My.Settings.MessageErreurRg = ""

        End If



        If (My.Settings.TypeMessageRg = 3 And WebBrowser3.IsBusy = False) Then

            ' Le BackGroundWorkerRG a récupéré la 1ere partie des résultats de la Recherche

            Try
                Dim flux As FileStream
                Dim ecriture As StreamWriter


                flux = File.Create(My.Application.Info.DirectoryPath & "\rg.html")
                ecriture = New StreamWriter(flux)
                ecriture.Write(My.Settings.PageHtmlRG)

                ecriture.Close()

                WebBrowser3.Url = New System.Uri(My.Application.Info.DirectoryPath & "\rg.html") 'Lit le fichier de résultat de Recherche

                WebBrowser3.Visible = True

            Catch ex As System.IO.IOException
                Exit Sub
            End Try

            My.Settings.TypeMessageRg = 0
            My.Settings.MessageErreurRg = ""
            My.Settings.PageHtmlRS = ""
            Exit Sub
        End If


        If (My.Settings.TypeMessageRg = 4 And WebBrowser3.IsBusy = False) Then

            ' Le BackGroundWorkerRG a récupéré la 2ème et dernière partie des résultats de la Recherche

            Try
                Dim flux As FileStream
                Dim ecriture As StreamWriter


                flux = File.Create(My.Application.Info.DirectoryPath & "\rg.html")
                ecriture = New StreamWriter(flux)
                ecriture.Write(My.Settings.PageHtmlRG)

                ecriture.Close()

                WebBrowser3.Url = New System.Uri(My.Application.Info.DirectoryPath & "\rg.html") 'Lit le fichier de résultat de Recherche
            Catch ex As System.IO.IOException
                Exit Sub
            End Try


            My.Settings.TypeMessageRg = 0
            My.Settings.MessageErreurRg = ""
            My.Settings.PageHtmlRS = ""
            TimerEtatRG.Stop()

        End If


        If (My.Settings.TypeMessageRg = 5 And WebBrowser3.IsBusy = False) Then
            Etat_Avancement(Couleur_fond_résultat_Recherche_Guidée, WebBrowser3, LangueLogiciel, 0, "")
            My.Settings.TypeMessageRg = 0
        End If

    End Sub

    Private Sub BackgroundWorker_Diagnostic_DoWork(sender As System.Object, e As System.ComponentModel.DoWorkEventArgs) Handles BackgroundWorker_Diagnostic.DoWork
        'on lance le process de diagnostic

        'Shell(My.Application.Info.DirectoryPath & "\Diagnostic_connexion.exe", AppWinStyle.NormalFocus, True, 60000)

        Dim s As String
        Dim p1 As New Process



        With p1.StartInfo
            .WorkingDirectory = My.Application.Info.DirectoryPath
            .FileName = "Diagnostic_connexion.exe"
            .Arguments = ""
            .UseShellExecute = False
            .RedirectStandardError = True
            .RedirectStandardInput = True
            .RedirectStandardOutput = True
            .WindowStyle = ProcessWindowStyle.Normal
            .CreateNoWindow = True

        End With

        Try

            p1.Start()
            s = p1.StandardOutput.ReadToEnd()
            p1.WaitForExit(40000)

        Catch ex As Exception
            s = ex.ToString
            MsgBox(s)
            p1.Close()
        End Try

        p1.Dispose()

    End Sub



    Private Sub Timer_Etat_Diagnostic_Tick(sender As System.Object, e As System.EventArgs) Handles TimerEtatDiagnostic.Tick
        Try

            Dim Résultat_dignostic_OK As Boolean = False

            If (File.Exists(My.Application.Info.DirectoryPath & "\Resultat_Diagnostic_OK.txt")) Then

                ' Nous avons besoin de lire dans une liste.
                Dim List_Résultat_diagnostic_txt As New List(Of String)

                ' ouverture du fichier txt avec l'état Using.
                Using r As StreamReader = New StreamReader(My.Application.Info.DirectoryPath & "\Resultat_Diagnostic_OK.txt")
                    'Stockage du contenu dans une chaîne.
                    Dim line As String

                    'Lecture de la première ligne 
                    line = r.ReadLine

                    ' Boucle sur chaque ligne dans le fichier,
                    ' Tant que list n'est pas Nothing.
                    Do While (Not line Is Nothing)
                        'Ajouter cette ligne à list
                        List_Résultat_diagnostic_txt.Add(line)
                        ' Afficher à la console.
                        'Console.WriteLine(line)

                        ' Lecture de la ligne suivante.
                        line = r.ReadLine
                    Loop
                    r.Close()
                End Using


                Dim T As Integer = 0
                Dim Result_str As String = ""
                '<RESULT>True</RESULT>
                '<RESULT>False</RESULT>
                For T = 0 To (List_Résultat_diagnostic_txt.Count - 1)

                    If (InStr(List_Résultat_diagnostic_txt(T).ToUpper, "<RESULT>".ToUpper) > 0) Then
                        Result_str = List_Résultat_diagnostic_txt(T)
                        Result_str = Result_str.Replace("<RESULT>", "")
                        Result_str = Result_str.Replace("</RESULT>", "")

                        If Result_str.ToUpper = "TRUE" Then
                            Résultat_dignostic_OK = True
                        End If


                        If Result_str.ToUpper = "FALSE" Then
                            Résultat_dignostic_OK = False
                        End If

                    End If

                Next T

            Else
                Exit Sub
            End If

            'c'est sûr il y a un résultat de diagnostic
            'on peut arrêter le Timer
            TimerEtatDiagnostic.Stop()
            TimerEtatDiagnostic.Interval = 3600000

            'on peut également supprimer le fichier de résultat
            Microsoft.VisualBasic.FileIO.FileSystem.DeleteFile(My.Application.Info.DirectoryPath & "\Resultat_Diagnostic_OK.txt")

            '
            'Valeur de Langue
            ' 0 = Allemand
            ' 1 = Anglais
            ' 2 = Espagnole
            ' 3 = Français
            ' 4 = Italien


            ' on va faire parler la synthèse vocale e.g : 'Le diagnostique de cette connexion a échoué'
            'MsgBox(lecture_du_resultat)
            If (My.Settings.Activation_Synthese_Vocale = True And Résultat_dignostic_OK = False) Then

                Dim phrase_à_dire As String = ""



                ' Me.xvoice.Select(My.Settings.Index_Moteur_Synthèse_Vocale)


                'Début SAPI 5.4
                Select Case Me.LangueLogiciel

                    Case 0
                        'Allemand
                        phrase_à_dire = "Der Diagnostische dieser Verbindung ist ein Misserfolg"

                    Case 1
                        'Americain Anglais
                        phrase_à_dire = "The diagnostic of this connection is a failure"

                    Case 2
                        'Espagnole
                        phrase_à_dire = "El diagnostico de esta conexion es un fracaso"

                    Case 3
                        'Français
                        phrase_à_dire = "Le diagnostic de cette connexion a aichouai"

                    Case 4
                        'Italien
                        phrase_à_dire = "Il diagnostico di questa connessione e un insuccesso"

                    Case Else

                End Select



                modification_commandes_SAPI5_TEXTE_A_DIRE(phrase_à_dire, My.Application.Info.DirectoryPath)

                'Fin SAPI 5.4

                'My.Settings.Texte_à_dire = phrase_à_dire
                'Me.Parle_Click(lecture_vocale_du_resultat, a)
            End If

            'Valeur de Langue
            ' 0 = Allemand
            ' 1 = Anglais
            ' 2 = Espagnole
            ' 3 = Français
            ' 4 = Italien

            ' on va faire parler la synthèse vocale e.g : 'Le diagnostique de cette connexion a échoué'
            'MsgBox(lecture_du_resultat)
            If (My.Settings.Activation_Synthese_Vocale = True And Résultat_dignostic_OK = True) Then

                Dim phrase_à_dire As String = ""



                Select Case Me.LangueLogiciel


                    Case 0
                        'Allemand
                        phrase_à_dire = "Das Diagnostische dieser Verbindung hat fertiggebracht"

                    Case 1
                        'Americain Anglais
                        phrase_à_dire = "The diagnostic of this connection made a success"

                    Case 2
                        'Espagnole
                        phrase_à_dire = "diagnostico de esta conexion consiguio"

                    Case 3
                        'Français
                        phrase_à_dire = "Le diagnostique de cette connexion est un suxai"

                    Case 4
                        'Italien
                        phrase_à_dire = "Il diagnostico di questa connessione e riuscito"

                    Case Else

                End Select


                modification_commandes_SAPI5_TEXTE_A_DIRE(phrase_à_dire, My.Application.Info.DirectoryPath)


            End If ' Fin SAPI 5.4



            'Actuellement on affiche le message d'indisponibilité du serveur (onglet aide)
            'dans la langue logiciel et le test du diagnostic a bien fonctionné il a retourné un bon fonctionnement
            'du serveur on peut dans ce cas modifier le message en SERVEUR DISPONIBLE le faire passer en vert
            'et recharger collections_menus

            'Message_utilisateur 55  => SERVEUR NON DISPONIBLE
            If (Résultat_dignostic_OK = True And SERVEUR_DISPONIBLE.Text = Ajout_Suppression_connexion_base.MessageUtilisateur(55, Me.LangueLogiciel).ToString) Then

                'On fait passer le message de indisponible à Disponible
                SERVEUR_DISPONIBLE.Text = Ajout_Suppression_connexion_base.MessageUtilisateur(53, Me.LangueLogiciel)

                'on passe la couleur du texte en vert
                SERVEUR_DISPONIBLE.ForeColor = Color.Green

                'le serveur est disponible pour recharger les menus
                Me.Chargement_Collections_Menus(sender, Me.connexion)  'ReChargement des collections des menus
                Me.chargements_constructeurs(connexion)        'Rechargement des constructeurs

                Dim retour_ok As Boolean = False
                retour_ok = Ajout_Suppression_connexion_base.ProcédureEntreServeurEtDonnéesLocale(Me.mysql_connection1, Me.connexion, _
                Me.Nom_de_la_base.Text, Me.Adresse_Serveur.Text, _
                 Me.LangueLogiciel)
                Me.ButtonActualiseEtatServeurMysql_Click("PREMIERE CONNEXION", e) 'va réactuliser l'état de ce serveur dans l'aide

            End If


            'Actuellement on affiche le message de disponibilité du serveur (onglet aide)
            'dans la langue logiciel et le test du diagnostic a retourné un mauvais fonctionnement
            'du serveur on peut dans ce cas modifier le message en SERVEUR INDISPONIBLE le faire passer en vert
            'et recharger collections_menus

            'Message_utilisateur 53  => SERVEUR DISPONIBLE
            If (Résultat_dignostic_OK = False And SERVEUR_DISPONIBLE.Text = Ajout_Suppression_connexion_base.MessageUtilisateur(53, Me.LangueLogiciel).ToString) Then

                'On fait passer le message de indisponible à Indisponible
                SERVEUR_DISPONIBLE.Text = Ajout_Suppression_connexion_base.MessageUtilisateur(55, Me.LangueLogiciel)

                'on passe la couleur du texte en rouge
                SERVEUR_DISPONIBLE.ForeColor = Color.Red



            End If

        Catch ex As Exception
        End Try

    End Sub

