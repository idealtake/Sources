    Private Sub Timer_Etat_RA_Tick(sender As System.Object, e As System.EventArgs) Handles TimerEtatRA.Tick

        'Type_de_message
        '0 = pas de message
        '1 = Recherche Terminée
        '2 = Erreur
        '3 = BackGroundWorkerRA a construit la page HTML RS 1ère phase
        '4 = BackGroundWorkerRA a construit la page HTML RS 2ème phase
        '5 = En attente

        If My.Settings.TypeMessageRa = 0 Then
            'pas de message
            Exit Sub
        End If

        If My.Settings.TypeMessageRa = 1 Then
            ' Recherche Terminée
            Etat_Avancement(Couleur_fond_résultat_RA, WebBrowser2, LangueLogiciel, 1, "")

            'TimerEtatRA.Stop()
            My.Settings.TypeMessageRa = 0
            My.Settings.MessageErreurRa = ""
            Exit Sub
        End If

        If My.Settings.TypeMessageRa = 2 Then
            ' Erreur Recherche
            Etat_Avancement(Couleur_fond_résultat_RA, WebBrowser2, LangueLogiciel, 2, My.Settings.MessageErreurRa)

            TimerEtatRA.Stop()
            My.Settings.TypeMessageRa = 0
            My.Settings.MessageErreurRa = ""

        End If


        If (My.Settings.TypeMessageRa = 3 And WebBrowser2.IsBusy = False) Then

            ' Le BackGroundWorkerRA a récupéré la 1ere partie des résultats de la Recherche

            Try
                Dim flux As FileStream
                Dim ecriture As StreamWriter


                flux = File.Create(My.Application.Info.DirectoryPath & "\ra.html")
                ecriture = New StreamWriter(flux)
                ecriture.Write(My.Settings.MessageErreurDetailsEnregistrementW1)

                ecriture.Close()

                WebBrowser2.Url = New System.Uri(My.Application.Info.DirectoryPath & "\ra.html") 'Lit le fichier de résultat de Recherche

                WebBrowser2.Visible = True

                'on rend visible le menu zoom
                MenuStripZoomWeb2.Visible = True

            Catch ex As System.IO.IOException
                Exit Sub
            End Try

            My.Settings.TypeMessageRa = 0
            My.Settings.MessageErreurRa = ""
            My.Settings.MessageErreurDetailsEnregistrementW1 = ""
            Exit Sub
        End If


        If (My.Settings.TypeMessageRa = 4 And WebBrowser2.IsBusy = False) Then

            ' Le BackGroundWorkerRA a récupéré la 2ème et dernière partie des résultats de la Recherche

            Try
                Dim flux As FileStream
                Dim ecriture As StreamWriter


                flux = File.Create(My.Application.Info.DirectoryPath & "\ra.html")
                ecriture = New StreamWriter(flux)
                ecriture.Write(My.Settings.MessageErreurDetailsEnregistrementW1)

                ecriture.Close()

                WebBrowser2.Url = New System.Uri(My.Application.Info.DirectoryPath & "\ra.html") 'Lit le fichier de résultat de Recherche

                'on rend visible le menu zoom
                'MenuStrip2.Visible = True


                If WebBrowser2.Visible = False Then

                    ' On a cliqué sur le bouton Annuler
                    ' Alors qu'une recherche était en cours
                    ' Le WebBrowser2  n'est pas visible actuellement
                    ' Donc MenuStrip2 non plus

                    'on rend invisible le menu zoom
                    MenuStripZoomWeb2.Visible = False

                Else
                    ' ici par contre on a pas cliqué sur le bouton "Annuler" de RA
                    ' on doit rendre visible Menustrip2

                    MenuStripZoomWeb2.Visible = True

                End If

            Catch ex As System.IO.IOException
                Exit Sub
            End Try


            My.Settings.TypeMessageRa = 0
            My.Settings.MessageErreurRa = ""
            My.Settings.MessageErreurDetailsEnregistrementW1 = ""
            TimerEtatRA.Stop()

        End If


        If (My.Settings.TypeMessageRa = 5 And WebBrowser2.IsBusy = False) Then
            Etat_Avancement(Couleur_fond_résultat_RA, WebBrowser2, LangueLogiciel, 0, "")
            My.Settings.TypeMessageRa = 0
        End If

    End Sub

