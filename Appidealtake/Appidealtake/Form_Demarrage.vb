            Exit Sub
        End If




        If (Retour_Recherche.Erreur = False) Then


            'Type_de_message
            '0 = pas de message
            '1 = Recherche Terminée
            '2 = Erreur
            '3 = BackgroundWorkerDetailsEnregistrementW1 a construit la page HTML Détails Enregistrement


            My.Settings.TypeMessageDetailsEnregistrementW3 = 1  'Type_message = 1 => Recherche Terminée 

