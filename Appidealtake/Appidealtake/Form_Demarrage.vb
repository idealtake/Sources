    Function Trie_des_listes_RA(ByVal Liste_chrono_documents As List(Of Résultats_recherche_avancée), _
                                ByVal Liste_chrono_enregistrements As List(Of Résultats_recherche_avancée), _
                                ByVal Liste_chrono_références_à_inclure As List(Of Résultats_recherche_avancée), _
                                ByVal score_de_la_recherche As Integer)

        Dim Résultat_du_trie As New List(Of Résultats_recherche_avancée)

        'aparté en c
        'int i = 33;string binary = Convert.ToString(i, 2);
        'string hex = Convert.ToString(i, 16);
        'int binaryToInt = Convert.ToInt32(binary, 2);
        'int hexToInt = Convert.ToInt32(hex, 16);

        'Chaque liste contient entre 0 et n numéro de chrono

        ' score_de_la_recherche As Integer = 0
        ' chrono pour des Types de documents recherchés            ---> Score =2^1 = 2
        ' chrono pour des enregistrements                          ---> Score =2^2 = 4
        ' chrono pour des références à inclure                     ---> Score =2^3 = 8


        '-----

        'Vérification que le score est bien transmit avec la bonne valeur
        'MsgBox(score_de_la_recherche)
        'ça marche

        '-----

        ' on va tester dans score_de_la_recherche si le bit 2^1
        'MsgBox((score_de_la_recherche And 2).ToString)
        ' Retourne soit 0 si le test ne fonctionne pas ou 2 si il marche

        '-----


        ' e.g score_de_la_recherche = 2 
        ' la recherche porte sur le numéro d'enregistrement


        Dim SCORE_en_C1 As Boolean = False   'chrono pour des Types de documents recherchés   2
        Dim SCORE_en_C2 As Boolean = False   'chrono pour des enregistrements                 4
        Dim SCORE_en_C3 As Boolean = False   'chrono pour des références à inclure            8



        SCORE_en_C1 = Convert.ToString((score_de_la_recherche And 2), 2)
        SCORE_en_C2 = Convert.ToString((score_de_la_recherche And 4), 2)
        SCORE_en_C3 = Convert.ToString((score_de_la_recherche And 8), 2)


        Dim index_résultat As Integer = 0
        'Dim index_num_enregistrement As Integer = 0
        'Dim index_mots_clefs As Integer = 0
        'Dim index_référence_à_inclure As Integer = 0


        'MsgBox("C1=" & SCORE_en_C1.ToString & ";C2=" & SCORE_en_C2.ToString & _
        '       ";C3=" & SCORE_en_C3.ToString)

        ' si Liste_chrono_documents.Count = 0 alors on doit
        ' sortir immédiatement de la fonction avec Résultat_du_trie vide
        If ((Liste_chrono_documents.Count = 0) And (SCORE_en_C1 = "True")) Then
            'MsgBox("il faut sortir la recherche ne retourne rien dans Liste_chrono_documents")
            Résultat_du_trie.Clear()
            GoTo fin_du_trie
        End If

        ' La Liste_chrono_documents.Count > 0 alors on recopie
        ' le contenue de la liste dans Résultat_du_trie
        If ((Liste_chrono_documents.Count > 0) And (SCORE_en_C1 = "True")) Then
            'MsgBox("ici il y a des résultats dans Liste_chrono_documents")
            Résultat_du_trie = Liste_chrono_documents

        End If



        ' si Liste_chrono_enregistrements.Count = 0 alors on doit
        ' sortir immédiatement de la fonction avec Résultat_du_trie vide
        If ((Liste_chrono_enregistrements.Count = 0) And (SCORE_en_C2 = "True")) Then
            'MsgBox("il faut sortir la recherche Liste_chrono_enregistrements ne retourne rien")
            Résultat_du_trie.Clear()
            GoTo fin_du_trie
        End If

        ' La Liste_chrono_enregistrements.Count > 0 alors on recopie
        ' le contenue de la liste dans Résultat_du_trie
        If ((Liste_chrono_enregistrements.Count > 0) And (SCORE_en_C2 = "True")) Then
            'MsgBox("ici il y a des résultats Liste_chrono_enregistrements")

            If (SCORE_en_C1 = "False") Then
                ' il n'y avait pas de recherche sur les chrono documents
                Résultat_du_trie = Liste_chrono_enregistrements
            Else
                'ici il faut faire le trie entre Résultat_du_trie (chrono documents) et chrono enregistrements

                Dim item_0 As Résultats_recherche_avancée
                Dim test_0 As String = ""
                Dim compte_0 As Integer = 0
                compte_0 = CInt(Résultat_du_trie.Count)


                Do Until index_résultat = compte_0


                    item_0 = Résultat_du_trie.Item(index_résultat)
                    test_0 = Liste_chrono_enregistrements.Contains(item_0)

                    'test_0 contient soit "True" ou "False"
                    'Si c'est "True" le numéro de chrono est présent dans Résultat_du_trie et dans Liste_chrono_enregistrements
                    'Si c'est "False" le numéro de chrono est présent dans Résultat_du_trie et absent de Liste_chrono_enregistrements
                    'Dans ce cas précis il faut enlever l'item de la liste Résultat_du_trie

                    'MsgBox(item_0.Chrono.ToString & ": contenue dans Liste_mots_clefs? ->  " & test_0)

                    If (test_0 = "False") Then
                        'ici on supprime l'item absent de la liste en cours de test de la liste Résultat_du_trie
                        Résultat_du_trie.Remove(item_0)
                        compte_0 = CInt(Résultat_du_trie.Count)
                        index_résultat = 0
                        If (Résultat_du_trie.Count = 0) Then
                            GoTo fin_du_trie
                        End If

                    Else
                        index_résultat += 1
                    End If


                Loop


            End If


        End If

        index_résultat = 0



        ' si Liste_chrono_références_à_inclure.Count = 0 alors on doit
        ' sortir immédiatement de la fonction avec Résultat_du_trie vide
        If ((Liste_chrono_références_à_inclure.Count = 0) And (SCORE_en_C3 = "True")) Then
            'MsgBox("il faut sortir la recherche Liste_chrono_références_à_inclure ne retourne rien")
