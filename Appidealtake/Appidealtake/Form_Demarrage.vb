    Private Sub BackgroundWorkerDetailsEnregistrementW3_DoWork(sender As System.Object, e As System.ComponentModel.DoWorkEventArgs) Handles BackgroundWorkerDetailsEnregistrementW3.DoWork


        Dim ArgumentsSQL As New ArgumentObjetDetailsEnregistrement
        'Dim Retour_des_liens As New RetourRechercheLiensDocuments

        ArgumentsSQL = e.Argument

