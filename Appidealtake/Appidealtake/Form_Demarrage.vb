    Private Function insert_data_in_table(ByVal Nom_de_la_base As String, _
                                          ByVal chaine_connexion As String, _
                                          ByVal Nom_de_la_table As String, _
                                          ByVal Définition_des_colonnes(,) As Object, _
                                          ByVal Nombre_de_colonnes As Integer, _
                                          ByVal Données(,) As Object)
        Dim requete As String = ""
        Dim temp As String = ""
        Dim CommandeInsertion As MySqlCommand
        Dim ConnexionClient As MySqlConnection

        'Try
        '
        requete = "INSERT INTO  " & Nom_de_la_base & "." & "`" & Nom_de_la_table & "` ("
        'exécution de la requête
        '(`INFORMATIONS_italien`,...) VALUES( " & DATA(T, 1) & ",
        Dim COL As Integer = 0

        For COL = 1 To Nombre_de_colonnes

            If COL < Nombre_de_colonnes Then
                requete = requete & "`" & Définition_des_colonnes(COL, 0).ToString & "`,"
            Else
                requete = requete & "`" & Définition_des_colonnes(COL, 0).ToString & "`) VALUES ("
            End If

        Next

        Dim Numéro_enregistrement As Integer = 1
        Dim Colonne_de_la_table As Integer = 1

        While True


            If (Définition_des_colonnes(Colonne_de_la_table, 1) = "Timestamp") Then

                'If (Données(Numéro_enregistrement, 1) = Nothing) Then
                'requete = requete & "null,"
                'Else
                requete = requete & "'" & Transformation_Date(Données(Numéro_enregistrement, Colonne_de_la_table)) & "'"
                'End If

                'chainesql_Insert_enregistrements = chainesql_Insert_enregistrements & "'" & Transformation_Date(DATA(T, 4)) & "',"



            End If


            If (Définition_des_colonnes(Colonne_de_la_table, 1) = "Integer") Then

                If (Données(Numéro_enregistrement, 1) = Nothing) Then
                    requete = requete & "null,"
                Else
                    requete = requete & Données(Numéro_enregistrement, Colonne_de_la_table)
                End If

            End If

            If ((Définition_des_colonnes(Colonne_de_la_table, 1) = "String") _
                Or (Définition_des_colonnes(Colonne_de_la_table, 1) = "Enum")) Then

                If (Données(Numéro_enregistrement, Colonne_de_la_table) = Nothing) Then
                    requete = requete & "null,"
                Else

                    temp = Données(Numéro_enregistrement, Colonne_de_la_table)

                    If (temp = " ") Then ' il contient simplement un espace
                        temp = temp
                    Else
                        temp = LTrim(temp)
                        temp = LTrim(temp)
                    End If

                    'ici on remplace de trois caractère
                    'Il y a quelques remplacement à réaliser

                    '   \r\n   --> \\r\\n	
                    temp = temp.Replace("\r\n", "\\r\\n")

                    '   "   --> \"	
                    temp = temp.Replace("""", "\""")

                    '   '   --> \'
                    temp = temp.Replace("'", "\'")

                    requete = requete & """" & temp & """"

                End If

            End If

            If (Colonne_de_la_table = Nombre_de_colonnes) Then
                Colonne_de_la_table = 1
                Numéro_enregistrement = Numéro_enregistrement + 1
                requete = requete & "),("
            Else
                Colonne_de_la_table = Colonne_de_la_table + 1
                requete = requete & ","
            End If

            If (Données(Numéro_enregistrement, 1) = Nothing) Then
                requete = requete & ");"
                Exit While
            End If

        End While

        requete = requete.Replace(",();", ";")
        requete = requete.Replace(",,", ",")
        requete = requete.Replace(",);", ");")
        requete = requete.Replace(",),", "),")

        If (InStr(requete, "(null);") > 0) Then
            GoTo fin
        End If

        'MsgBox(requete)
        'Exit Function

        CommandeInsertion = New MySqlCommand
        CommandeInsertion.CommandText = requete

        ConnexionClient = New MySqlConnection(chaine_connexion)
        CommandeInsertion.Connection = ConnexionClient

        ConnexionClient.Open()
        CommandeInsertion.ExecuteNonQuery() 'Insertion de données dans la table
        ConnexionClient.Close()
fin:

        'Catch ex As Exception
        'MessageBox.Show(ex.Message, "Erreur", MessageBoxButtons.OK, MessageBoxIcon.Error)
        'End Try

        Return True
    End Function

    Private Function Read_data_from_xml_file(ByVal chemin_et_Nom_de_la_table As String, _
                                             ByVal Nom_des_colonnes(,) As Object, _
                                             ByVal Nombre_de_colonnes As Integer)

        Dim temp As String
        Dim info_récupérée As String
        Dim T1, T2 As Integer
        Dim Données(30000, 60) As String
        Dim NbreLigneActive As Integer = 0

        temp = ""
        info_récupérée = ""
        T1 = 1
        T2 = 1
        Using reader As XmlReader = XmlReader.Create(chemin_et_Nom_de_la_table)
            reader.ReadStartElement(Nom_des_colonnes(0, 0).ToString)

            While reader.Read()



                For T1 = 1 To Nombre_de_colonnes

                    If reader.IsStartElement() Then
                        temp = reader.Name
                        temp = LTrim(temp)
                        temp = RTrim(temp)
                        If (temp = Nom_des_colonnes(T1, 0).ToString) Then
                            info_récupérée = reader.ReadString().ToString
                            Données(T2, T1) = info_récupérée
                        End If

                        If temp = "DATA" Then
                            T1 = 0
                        End If
                        If T1 = Nombre_de_colonnes Then
                            T2 += 1 : NbreLigneActive = T2
                            T1 = 0
                        End If

                    End If



                    reader.Read() ' Read the start tag.
                Next


            End While

        End Using

        NbreLigneActive -= 1
        Données(0, 0) = NbreLigneActive

        Return Données

    End Function

