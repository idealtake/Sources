    Function type_d_image(ByVal chemin_plus_image As String)

        If (InStr(chemin_plus_image.ToUpper, ".jpeg".ToUpper) > 0) Or (InStr(chemin_plus_image.ToUpper, ".jpg".ToUpper) > 0) Then
            Return "jpg"
        End If

        If (InStr(chemin_plus_image.ToUpper, ".exif".ToUpper) > 0) Then
            Return "exif"
        End If

        If (InStr(chemin_plus_image.ToUpper, ".bmp".ToUpper) > 0) Then
            Return "bmp"
        End If


        If (InStr(chemin_plus_image.ToUpper, ".emf".ToUpper) > 0) Then
            Return "emf"
        End If

        If (InStr(chemin_plus_image.ToUpper, ".gif".ToUpper) > 0) Then
            Return "gif"
        End If

        If (InStr(chemin_plus_image.ToUpper, ".ico".ToUpper) > 0) Then
            Return "ico"
        End If

        If (InStr(chemin_plus_image.ToUpper, ".png".ToUpper) > 0) Then
            Return "png"
        End If

        If (InStr(chemin_plus_image.ToUpper, ".tiff".ToUpper) > 0) Then
            Return "tiff"
        End If

        If (InStr(chemin_plus_image.ToUpper, ".wmf".ToUpper) > 0) Then
            Return "wmf"
        End If

        Return ""

    End Function

    Function GenerateThumbnail(ByVal image_chargée As Image)

        Dim imgThumb As Image = Nothing
        Dim ratio_image As Single = image_chargée.Width / image_chargée.Height
        Dim surface_image As Single = image_chargée.Width * image_chargée.Height
        Dim surface_reference As Single = 20000

        Try
            If (surface_image <= surface_reference) Then
                imgThumb = image_chargée
            End If

            If (surface_image > surface_reference) Then
                'If (image_chargée.Width > 200 And image_chargée.Height > 200) Then

                Dim Width_image As Integer = image_chargée.Width
                Dim height_image As Integer = image_chargée.Height
                Dim x As Single = 1

                While (Width_image > 250 Or height_image > 250 Or (Width_image * height_image > 30000))
                    Width_image = Width_image / x
                    height_image = height_image / x

                    If (x < 10) Then x = x + 0.01
                    If (x > 10 And x < 100) Then x = x + 0.2
                    If (x > 100) Then x = x + 1
                End While

                ' MsgBox(x)

                imgThumb = image_chargée.GetThumbnailImage(Width_image, height_image, Nothing, New IntPtr())

                'Else
                'imgThumb = image_chargée
                'End If
            End If

        Catch ex As Exception
            'MessageBox.Show("An error occured : " & ex.Message, "GenerateThumbnail()")
            imgThumb = image_chargée
        End Try

        Return imgThumb

    End Function

    Function Convertir_une_Image_Vers_Base64(ByVal nom_image As String, ByVal image_à_convertir As Image)

        Dim chaine_base64 As String = ""

        Try
            ', ByVal format_image As System.Drawing.Imaging.ImageFormat
            Dim format_image As System.Drawing.Imaging.ImageFormat = ImageFormat.Jpeg

            If (InStr(nom_image.ToUpper, ".jpeg".ToUpper) > 0) Or (InStr(nom_image.ToUpper, ".jpg".ToUpper) > 0) Then
                format_image = System.Drawing.Imaging.ImageFormat.Jpeg
            End If

            If (InStr(nom_image.ToUpper, ".exif".ToUpper) > 0) Then
                format_image = System.Drawing.Imaging.ImageFormat.Exif
            End If

            If (InStr(nom_image.ToUpper, ".bmp".ToUpper) > 0) Then
                format_image = System.Drawing.Imaging.ImageFormat.Bmp
            End If


            If (InStr(nom_image.ToUpper, ".bmp".ToUpper) > 0) Then
                format_image = System.Drawing.Imaging.ImageFormat.Bmp
            End If

            If (InStr(nom_image.ToUpper, ".emf".ToUpper) > 0) Then
                format_image = System.Drawing.Imaging.ImageFormat.Emf
            End If

            If (InStr(nom_image.ToUpper, ".gif".ToUpper) > 0) Then
                format_image = System.Drawing.Imaging.ImageFormat.Gif
            End If

            If (InStr(nom_image.ToUpper, ".ico".ToUpper) > 0) Then
                format_image = System.Drawing.Imaging.ImageFormat.Icon
            End If

            If (InStr(nom_image.ToUpper, ".png".ToUpper) > 0) Then
                format_image = System.Drawing.Imaging.ImageFormat.Png
            End If

            If (InStr(nom_image.ToUpper, ".tiff".ToUpper) > 0) Then
                format_image = System.Drawing.Imaging.ImageFormat.Tiff
            End If

            If (InStr(nom_image.ToUpper, ".wmf".ToUpper) > 0) Then
                format_image = System.Drawing.Imaging.ImageFormat.Wmf
            End If

            Dim FLUX_mémoire As New System.IO.MemoryStream

            Using (FLUX_mémoire)
                'convertir image en octets[]
                image_à_convertir.Save(FLUX_mémoire, format_image)
                Dim ImageOctets() As Byte = FLUX_mémoire.ToArray()


                'convertir octets[] en chaîne BASE64
                chaine_base64 = Convert.ToBase64String(ImageOctets)

            End Using

        Catch ex As Exception
            'MessageBox.Show("An error occured : " & ex.Message, "Image_Vers_Base64()")
        End Try

        Return chaine_base64

    End Function

