    Public Function Analyse_des_conditions_de_l_instruction_si(ByVal Instruction As String)

        Dim Test_Conditions_instruction As Boolean = False
        Dim Chaîne_des_conditions As String
        Dim Chaîne_des_conditions_sauvegarde As String
        Chaîne_des_conditions = ""
        Chaîne_des_conditions_sauvegarde = ""
        Dim longueur_entre_si_et_alors_instruction = 0
        Dim séparateur() As String = {"ET", "OU"}
        Dim Table_des_conditions()
        Dim Lien_entre_les_conditions() = {"", "", "", "", "", "", "", "", "", "", "", "", "", "", "", "", "", "", "", ""}
        Dim Position_ET As Integer = 0
        Dim Position_OU As Integer = 0

        Instruction = Instruction.ToUpper
        longueur_entre_si_et_alors_instruction = Instruction.IndexOf("ALORS ") - Instruction.IndexOf("SI ")

        Chaîne_des_conditions = Instruction.Substring(Instruction.IndexOf("SI ") + 4, longueur_entre_si_et_alors_instruction - 6)
        Chaîne_des_conditions_sauvegarde = Chaîne_des_conditions

        Chaîne_des_conditions = Chaîne_des_conditions.Replace(" ET ", "|")
        Chaîne_des_conditions = Chaîne_des_conditions.Replace(" OU ", "|")

        Table_des_conditions = Chaîne_des_conditions.Split("|") 'ici on a les différentes conditions

        Chaîne_des_conditions = Chaîne_des_conditions_sauvegarde
        Position_ET = Chaîne_des_conditions.IndexOf("ET")  'si = -1 alors pas de ET
        Position_OU = Chaîne_des_conditions.IndexOf("OU")  'si = -1 alors pas de OU

        'MsgBox(Position_ET & " " & Position_OU)
        Dim t As Integer = 0
        Dim position_occurence As Integer = 0

        If (Position_ET = -1) Then
            'pas de et

            If (Position_OU = -1) Then
                'pas de et pas de ou
                'ne pas remplir "Lien_entre_les_conditions"
                'on fait rien
            Else
                'pas de et mais il y a des ou
                'remplir "Lien_entre_les_conditions" avec les ou

                For t = 0 To 20
                    position_occurence = Chaîne_des_conditions.IndexOf(" OU ")
                    If (position_occurence > 0) Then
                        Lien_entre_les_conditions(t) = "OU"
                    End If

                    If (Chaîne_des_conditions.Length > position_occurence + 4) Then
                        Chaîne_des_conditions = Chaîne_des_conditions.Substring(position_occurence + 4)
                    Else
                        Exit For
                    End If

                Next

            End If
        Else
            'il y a des et

            If (Position_OU = -1) Then
                'il y a des et mais pas ou
                ' remplir "Lien_entre_les_conditions" avec les et

                For t = 0 To 20
                    position_occurence = Chaîne_des_conditions.IndexOf(" ET ")
                    If (position_occurence > 0) Then
                        Lien_entre_les_conditions(t) = "ET"
                    End If

                    If (Chaîne_des_conditions.Length > position_occurence + 4) Then
                        Chaîne_des_conditions = Chaîne_des_conditions.Substring(position_occurence + 4)
                    Else
                        Exit For
                    End If

                Next

            Else
                'il y a des et/ou
                'remplir "Lien_entre_les_conditions" avec les et/ou

                For t = 0 To 20
                    Position_ET = Chaîne_des_conditions.IndexOf(" ET ")
                    Position_OU = Chaîne_des_conditions.IndexOf(" OU ")

                    If (Position_ET <> -1 And Position_OU <> -1) Then

                        If (Position_ET < Position_OU) Then
                            Lien_entre_les_conditions(t) = "ET"

                            If (Chaîne_des_conditions.Length > Position_ET + 4) Then
                                Chaîne_des_conditions = Chaîne_des_conditions.Substring(Position_ET + 4)
                            Else
                                Exit For
                            End If

                        End If '(Position_ET < Position_OU)

                        If (Position_OU < Position_ET) Then

                            Lien_entre_les_conditions(t) = "OU"

                            If (Chaîne_des_conditions.Length > Position_OU + 4) Then
                                Chaîne_des_conditions = Chaîne_des_conditions.Substring(Position_OU + 4)
                            Else
                                Exit For
                            End If

                        End If '(Position_OU < Position_ET)

                    End If '(Position_ET <> -1 And Position_OU <> -1)


                    If (Position_ET = -1 And Position_OU > 0) Then

                        Lien_entre_les_conditions(t) = "OU"

                        If (Chaîne_des_conditions.Length > Position_OU + 4) Then
                            Chaîne_des_conditions = Chaîne_des_conditions.Substring(Position_OU + 4)
                        Else
                            Exit For
                        End If

                    End If '(Position_ET = -1 And Position_OU > 0)


                    If (Position_OU = -1 And Position_ET > 0) Then

                        Lien_entre_les_conditions(t) = "ET"

                        If (Chaîne_des_conditions.Length > Position_ET + 4) Then
