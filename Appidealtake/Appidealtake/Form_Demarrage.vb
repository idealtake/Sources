                My.Settings.MessageSupprimerEnregistrement = ""
                Exit Sub
            End If

            ' pause de 1 seconde
            'pour apercevoir les messages de la deuxième requête
            System.Threading.Thread.Sleep(pauseTime)



            My.Settings.TypeMessageSupprimerEnregistrement = 7 'Type_message = 7 => Suppression en cours
            My.Settings.MessageSupprimerEnregistrement = ""


            ' pause de 1 seconde
            'pour apercevoir les messages de le message suppression en cours...
            System.Threading.Thread.Sleep(pauseTime)

