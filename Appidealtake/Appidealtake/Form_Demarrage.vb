    Public Function Controle_chaine(ByVal Chaine_a_controler As String)

        Dim Erreur_contrôle As Boolean = False
        Dim indice_boucle As Integer = 0
        Dim Table_caractères() = {"+", "@", "^", "`", "§", "¨", "²", "€", "?", ",", _
                                  "}", "{", "~", "$", "£", "È", "ö", "ï", "ë", "ä", _
                                  "ü", "â", "û", "ô", "î", "ê", ".", "-", "+", "_", _
                                  "*", ".", "!", "/", "\", ":", "#", """", "'", "é", _
                                  "(", "è", "ç", "à", "$", "ù", "%", "<", ">", "&"}


        For indice_boucle = 0 To (Table_caractères.Length - 1)
            If (InStr(Chaine_a_controler, Table_caractères(indice_boucle)) > 0) Then
                Erreur_contrôle = True
                GoTo label1
            End If

        Next

label1:

        Return Erreur_contrôle

    End Function


