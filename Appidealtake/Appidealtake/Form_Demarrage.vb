    Private Function Initialiser_langue_logiciel(ByVal Nom_de_l_object As Object)

        Dim Langue As Integer
        Langue = My.Settings.Langue_logiciel
        Ajout_Suppression_connexion_base.ListeObjectATraduire()

        'Valeur de Langue
        ' 0 = Allemand
        ' 1 = Anglais
        ' 2 = Espagnole
        ' 3 = Français
        ' 4 = Italien

        Select Case Langue
            Case 0
                Nom_de_l_object.text = " - Deutsche Sprache -"

            Case 1
                Nom_de_l_object.text = " - English language -"

            Case 2
                Nom_de_l_object.text = " - Lengua Españoles -"

            Case 3
                Nom_de_l_object.text = " - Langue Française -"

            Case 4
                Nom_de_l_object.text = " - Lingua Italiano -"

            Case Else

        End Select


        Return Langue
    End Function

