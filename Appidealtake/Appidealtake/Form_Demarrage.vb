    Function Transcodage_Menu(ByVal Tableau As Object, ByVal Texte_à_transcoder As String)

        '' Menu Systeme(100,10)  nota position(0,  pas utilisé
        '' INDICE_AUTO Tableau(1,x)
        '' INFOS_VISIBLE_LOGICIEL Tableau(2,x)
        '' REQUETE_REELLE  Tableau(3,x)
        ''
        '' structure sous Mysql
        ''+--------------------------+------------------+------+-----+---------+----------------+
        ''| Field                    | Type             | Null | Key | Default | Extra          |
        ''+--------------------------+------------------+------+-----+---------+----------------+
        ''| INDICE_AUTO              | int(10) unsigned | NO   | PRI | NULL    | auto_increment |
        ''| INFOS_VISIBLE_LOGICIEL   | text             | YES  |     | NULL    |                |
        ''| REQUETE_REELLE           | text             | YES  |     | NULL    |                |
        ''| VALEUR_AUDIO_VIDEO       | text             | YES  |     | NULL    |                |
        ''| FIGER_AUDIO_VIDEO        | text             | YES  |     | NULL    |                |
        ''| SUPPRIMER_DE_AUDIO_VIDEO | text             | YES  |     | NULL    |                |
        ''+--------------------------+------------------+------+-----+---------+----------------+

        Dim Texte_transcodé As String = ""


        Dim T1 As Integer
        Dim position_du_texte As Integer = 0

        For T1 = 0 To 100

            If (Tableau(T1, 2) = Texte_à_transcoder) Then 'vérifie si le texte à transcoder existe en colonne 2
                Exit For
            End If

        Next

        If (T1 > 99) Then
            'dans ce cas le chargement des infos ne s'est pas faite correctement
            'on utilise les valeurs de la collection menu déja existante
            'des Valeurs Reelles pas de transcodage nécessaire
            Texte_transcodé = Texte_à_transcoder
        Else
            Texte_transcodé = Tableau(T1, 3)  'valeur REELLE récupéré dans le tableau

        End If
        If (Texte_transcodé = "NULL") Then
            'le trancodage à retourné la valeur NULL
            'elle doit être interprété comme une chaîne vide
            'on doit renvoyer une chaîne vide
            Texte_transcodé = ""
        End If


        Return Texte_transcodé
    End Function


