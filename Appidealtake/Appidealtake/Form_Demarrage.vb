    Function Copie_de_fichier(ByVal sender As System.Object, ByVal e As System.EventArgs, ByVal ENR_NUM_SCH As String)

        'M.R.U
        'permet la sélection d'un fichier sur le PC
        'et de le transférer sur l'ordinateur
        'utilisation anciennement de la table SCHEMA

        Dim file1 As String = ""
        Dim file2 As String = ""
        Dim file3 As String = ""
        Dim Source1 As String = ""
        Dim Destination1 As String = ""

        OpenFileDialog1.Dispose()
        OpenFileDialog1.Reset()
        OpenFileDialog1.Title = "Sélectionnez le Fichier source"

        If (My.Settings.Chemin_Source1 <> "") Then
            If (My.Computer.FileSystem.DirectoryExists(My.Settings.Chemin_Source1)) Then
                OpenFileDialog1.InitialDirectory = My.Settings.Chemin_Source1
                ' MsgBox(OpenFileDialog1.FileName)
            End If

        End If





        Try

            OpenFileDialog1.ShowDialog()
            file1 = OpenFileDialog1.FileName
            'file1 = OpenFileDialog1.InitialDirectory & OpenFileDialog1.FileName

            If (Microsoft.VisualBasic.Strings.Len(file1) = 0) Then
                MsgBox("Vous devez sélectionner un fichier Source et un chemin Source.", MsgBoxStyle.Information, "Information")
                ENR_NUM_SCH = ""
                Return False
                Exit Function   ' pas de fichier sélectionné pour la source
            End If


            If My.Computer.FileSystem.FileExists(file1) Then
                Source1 = file1
                Dim pos2 As Integer = 0
                Dim pos3 As Integer = 0
                Dim pos4 As Integer = 0
                pos2 = file1.LastIndexOf("\")                           'position de la derniere occurence du caractère \
                pos3 = Microsoft.VisualBasic.Strings.Len(file1)         'longueur de la chaine file1
                pos4 = pos3 - pos2 - 1                                  'longueur de la chaine à récupérer
                file1 = Microsoft.VisualBasic.Strings.Right(file1, pos4)

                'ENR_NUM_SCH.Text = file1

                'Sauve le chemin courant de la source récupérer 
                'à la prochaine utilisation du logiciel

                file3 = Microsoft.VisualBasic.Strings.Left(OpenFileDialog1.FileName, (pos2 + 1))
                My.Settings.Chemin_Source1 = file3


                'MsgBox(My.Settings.Chemin_Source1)
            End If

            SaveFileDialog1.Title = "Sélectionnez la destination de la copie du Fichier"
            SaveFileDialog1.FileName = file1
            SaveFileDialog1.ShowDialog()
            file2 = SaveFileDialog1.InitialDirectory & SaveFileDialog1.FileName
            Destination1 = file2

            'c'est le nom du fichier de destination
            'que l'on prend en NUM SCHEMA
            ENR_NUM_SCH = file2


            If (Microsoft.VisualBasic.Strings.Len(file2) = 0) Then
                ENR_NUM_SCH = ""
                MsgBox("Vous devez sélectionner un fichier et un chemin de Destination.", MsgBoxStyle.Information, "Information")
                Return False
                Exit Function   ' pas de fichier sélectionné pour la destination
            End If


            'MsgBox(Source1)
            'MsgBox(Destination1)

            'on copie le fichier source vers destination

            System.IO.File.Copy(Source1, Destination1)

        Catch ex As Exception
            MessageBox.Show(ex.Message, "Erreur_1 Copie_de_fichier()", MessageBoxButtons.OK, MessageBoxIcon.Error)
            ENR_NUM_SCH = ""
        End Try

        OpenFileDialog1.Reset()
        OpenFileDialog1.Dispose()
        SaveFileDialog1.Reset()
        SaveFileDialog1.Dispose()

        Return True

    End Function


