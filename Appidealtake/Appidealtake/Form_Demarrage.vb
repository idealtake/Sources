                        End If
                    End While
                End Using

            Catch ex As Exception
                MsgBox("Erreur_1 Function Chargement_des_connexions() " & ex.Message, MsgBoxStyle.Critical)

                'Log Idealtake
                Dim infosLog As String = ""
                infosLog = "Erreur_1 Function Chargement_des_connexions() :Not OK " & vbCrLf & ex.Message
