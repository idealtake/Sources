<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <configSections>
        <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
            <section name="TransfertIdealtake.My.MySettings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false"/>
        </sectionGroup>
    </configSections>
    <system.diagnostics>
        <sources>
            <!-- Cette section définit la configuration de l'enregistrement dans le fichier journal de My.Application.Log -->
            <source name="DefaultSource" switchName="DefaultSwitch">
                <listeners>
                    <add name="FileLog"/>
                    <!-- Supprimez les marques de commentaire dans la section suivante pour écrire dans le journal des événements de l'application -->
                    <!--<add name="EventLog"/>-->
                </listeners>
            </source>
        </sources>
        <switches>
            <add name="DefaultSwitch" value="Information"/>
        </switches>
        <sharedListeners>
            <add name="FileLog" type="Microsoft.VisualBasic.Logging.FileLogTraceListener, Microsoft.VisualBasic, Version=8.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a, processorArchitecture=MSIL" initializeData="FileLogWriter"/>
            <!-- Supprimez les marques de commentaire dans la section suivante et remplacez APPLICATION_NAME par le nom de votre application à écrire dans le journal des événements de l'application -->
            <!--<add name="EventLog" type="System.Diagnostics.EventLogTraceListener" initializeData="APPLICATION_NAME"/> -->
        </sharedListeners>
    </system.diagnostics>
    <userSettings>
        <TransfertIdealtake.My.MySettings>
            <setting name="Langue_logiciel" serializeAs="String">
                <value>3</value>
            </setting>
            <setting name="TypeMessageChargementTypeDocument" serializeAs="String">
                <value>0</value>
            </setting>
            <setting name="MessageChargementTypeDocument" serializeAs="String">
                <value/>
            </setting>
            <setting name="TypeMessageChargementListeDocuments" serializeAs="String">
                <value>0</value>
            </setting>
            <setting name="MessageChargementListeDocuments" serializeAs="String">
                <value/>
            </setting>
            <setting name="TypeMessageMaintenanceTransfert" serializeAs="String">
                <value>0</value>
            </setting>
            <setting name="MessageMaintenanceTransfert" serializeAs="String">
                <value/>
            </setting>
            <setting name="TypeMessageSupprimeDocument" serializeAs="String">
                <value>0</value>
            </setting>
            <setting name="MessageSupprimeDocument" serializeAs="String">
                <value/>
            </setting>
            <setting name="TypeMessageAjoutDocument" serializeAs="String">
                <value>0</value>
            </setting>
            <setting name="MessageAjoutDocument" serializeAs="String">
                <value/>
            </setting>
        </TransfertIdealtake.My.MySettings>
    </userSettings>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/></startup></configuration>
